---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Button } from "@/components/ui/button";
import Breadcrumbs from "@/components/seo/Breadcrumbs.astro";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";
import { MapPin, Clock, Check, Phone, MessageCircle, IndianRupee, Navigation, Star, Zap } from "lucide-react";

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Routes", url: "/routes" },
  { name: "Hadapsar to Mumbai Airport", url: "/routes/hadapsar-to-mumbai-airport" }
]);

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "name": "Hadapsar to Mumbai Airport Electric SUV Cab Service",
  "serviceType": "Airport Transfer Service",
  "provider": {
    "@type": "Organization",
    "name": "GreenEVCabs",
    "telephone": "+91-7498264215"
  },
  "areaServed": [
    {
      "@type": "Place",
      "name": "Hadapsar, Pune"
    },
    {
      "@type": "Airport",
      "name": "Mumbai Airport",
      "iataCode": "BOM"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2999",
    "priceCurrency": "INR"
  }
};
---

<MainLayout
  title="Hadapsar to Mumbai Airport Electric SUV Cab ₹2,999 | 7 Seater"
  description="Book 7-seater electric SUV cab from Hadapsar to Mumbai Airport at ₹2,999. Pickup from Hadapsar, Magarpatta, Kharadi, EON IT Park. Zero surge, flight tracking, 24/7. Save ₹700."
  keywords={[
    "hadapsar to mumbai airport cab",
    "hadapsar mumbai airport taxi",
    "electric suv cab hadapsar to mumbai airport",
    "7 seater cab hadapsar mumbai airport",
    "hadapsar to bom airport cab",
    "magarpatta mumbai airport cab",
    "kharadi to mumbai airport",
    "cheapest cab hadapsar mumbai airport"
  ]}
>
  <Breadcrumbs items={[
    { name: "Home", href: "/" },
    { name: "Routes", href: "/routes" },
    { name: "Hadapsar to Mumbai Airport", href: "/routes/hadapsar-to-mumbai-airport" }
  ]} />

  <StructuredData data={[breadcrumbSchema, serviceSchema]} />

  <div class="container px-4 py-12 md:py-16">
    <!-- Hero -->
    <div class="text-center mb-12">
      <Badge variant="secondary" className="mb-4">
        Hadapsar Pickup • Zero Surge • 24/7 • Corporate Packages
      </Badge>
      <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold tracking-tight mb-6">
        Hadapsar to Mumbai Airport Electric SUV Cab ₹2,999
      </h1>
      <p class="text-xl md:text-2xl text-muted-foreground max-w-4xl mx-auto leading-relaxed mb-8">
        Book premium 7-seater electric SUV cab from Hadapsar to Mumbai Airport (BOM) at just ₹2,999. Pickup from Hadapsar, Magarpatta City, Kharadi, EON IT Park areas. Spacious Kia Carens EV & BYD E-MAX 7. Corporate packages with GST invoicing.
      </p>

      <!-- Trust Signals -->
      <div class="flex flex-wrap justify-center gap-6 mb-8">
        <div class="flex items-center gap-2">
          <Star className="h-5 w-5 text-yellow-500 fill-yellow-500" />
          <span class="font-semibold">4.9/5 Rating</span>
        </div>
        <div class="flex items-center gap-2">
          <Zap className="h-5 w-5 text-green-600" />
          <span class="font-semibold">100% Electric</span>
        </div>
        <div class="flex items-center gap-2">
          <Check className="h-5 w-5 text-primary" />
          <span class="font-semibold">GST Invoice</span>
        </div>
      </div>

      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <a href="https://wa.me/917498264215?text=Hi%2C%20I%20want%20to%20book%20electric%20SUV%20cab%20from%20Hadapsar%20to%20Mumbai%20Airport%0APickup%20Location%3A%20Hadapsar%0ADate%20%26%20Time%3A%20%0APassengers%3A%20" target="_blank">
          <Button size="lg" className="bg-green-500 hover:bg-green-600 text-white">
            <MessageCircle className="mr-2 h-5 w-5" />
            Book on WhatsApp Now
          </Button>
        </a>
        <a href="tel:+917498264215">
          <Button size="lg" variant="outline">
            <Phone className="mr-2 h-5 w-5" />
            Call +91 74982 64215
          </Button>
        </a>
      </div>
    </div>

    <!-- Route Info -->
    <div class="mb-16 bg-primary/5 rounded-lg p-8">
      <div class="grid md:grid-cols-4 gap-6 text-center">
        <div>
          <MapPin className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Pickup Area</h3>
          <p class="text-2xl font-bold text-primary">Hadapsar</p>
          <p class="text-sm text-muted-foreground">East Pune</p>
        </div>
        <div>
          <Navigation className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Distance</h3>
          <p class="text-2xl font-bold text-primary">160 km</p>
          <p class="text-sm text-muted-foreground">Via Expressway</p>
        </div>
        <div>
          <Clock className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Journey Time</h3>
          <p class="text-2xl font-bold text-primary">2.5-3 hrs</p>
          <p class="text-sm text-muted-foreground">Traffic dependent</p>
        </div>
        <div>
          <IndianRupee className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Fixed Fare</h3>
          <p class="text-2xl font-bold text-primary">₹2,999</p>
          <p class="text-sm text-muted-foreground">Tolls included</p>
        </div>
      </div>
    </div>

    <!-- Hadapsar Pickup Locations -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Pickup from All Hadapsar & East Pune Areas</h2>
      <p class="text-center text-muted-foreground mb-8 max-w-3xl mx-auto">
        We provide doorstep pickup from all residential, commercial, and IT Park areas in East Pune
      </p>

      <div class="grid md:grid-cols-3 gap-6 max-w-5xl mx-auto">
        <Card>
          <CardHeader>
            <CardTitle>Hadapsar Main Area</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Hadapsar Gadital</li>
              <li>• Magarpatta City</li>
              <li>• Magarpatta Road</li>
              <li>• Mundhwa Road</li>
              <li>• Residential societies</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>IT Parks & Corporate</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• EON IT Park Kharadi</li>
              <li>• World Trade Center</li>
              <li>• Cybercity Magarpatta</li>
              <li>• IT offices Kharadi</li>
              <li>• Corporate hubs</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Nearby Areas</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Kharadi</li>
              <li>• Wagholi</li>
              <li>• Fatima Nagar</li>
              <li>• Wanowrie</li>
              <li>• Camp proximity</li>
            </ul>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Pricing -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Hadapsar to Mumbai Airport Cab Fare</h2>

      <div class="grid md:grid-cols-2 gap-6 max-w-4xl mx-auto">
        <Card className="border-2 border-primary">
          <CardHeader className="bg-primary/5">
            <CardTitle class="flex items-center justify-between">
              Kia Carens EV (7-Seater SUV)
              <Badge>Best Value</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹2,999</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Spacious 7-seater electric SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">GST invoice for corporate expense</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Zero surge pricing - 24/7 same rate</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Flight tracking included</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20Kia%20Carens%20Hadapsar%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full">Book Now ₹2,999</Button>
            </a>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="flex items-center justify-between">
              BYD E-MAX 7 (Premium SUV)
              <Badge variant="secondary">Luxury</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹5,200</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Premium 7-seater luxury SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">530 km extended range</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Captain seats & extra legroom</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Perfect for executive travel</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20BYD%20E-MAX%207%20Hadapsar%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full" variant="outline">Book Premium SUV</Button>
            </a>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Corporate Section -->
    <div class="mb-16 bg-gradient-to-r from-blue-50 to-purple-50 dark:from-blue-950/20 dark:to-purple-950/20 rounded-lg p-8">
      <h2 class="text-3xl font-bold mb-6 text-center">Corporate Electric Cab Service - Hadapsar IT Parks</h2>
      <p class="text-center text-muted-foreground mb-8 max-w-3xl mx-auto">
        Trusted by leading companies in Magarpatta City, EON IT Park, and Kharadi for employee airport transfers
      </p>

      <div class="grid md:grid-cols-2 gap-6">
        <div>
          <h3 class="font-semibold mb-3">Corporate Benefits</h3>
          <ul class="space-y-2 text-sm">
            <li class="flex items-start gap-2">
              <Check className="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
              <span>GST invoices for all bookings</span>
            </li>
            <li class="flex items-start gap-2">
              <Check className="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
              <span>Monthly billing options available</span>
            </li>
            <li class="flex items-start gap-2">
              <Check className="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
              <span>Volume discounts for regular bookings</span>
            </li>
            <li class="flex items-start gap-2">
              <Check className="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
              <span>Dedicated account manager</span>
            </li>
          </ul>
        </div>

        <div>
          <h3 class="font-semibold mb-3">Perfect For</h3>
          <ul class="space-y-2 text-sm">
            <li>• Employee airport transfers</li>
            <li>• Client pickup & meetings</li>
            <li>• Business travel reimbursement</li>
            <li>• Team outings & events</li>
            <li>• Eco-friendly corporate transport</li>
          </ul>
        </div>
      </div>
    </div>

    <!-- FAQ -->
    <div class="mb-16">
      <h2 class="text-3xl font-bold mb-8 text-center">FAQs - Hadapsar to Mumbai Airport Cab</h2>

      <div class="space-y-4 max-w-4xl mx-auto">
        <Card>
          <CardHeader>
            <CardTitle class="text-lg">How much is cab from Hadapsar to Mumbai Airport?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Electric SUV cab from Hadapsar to Mumbai Airport costs ₹2,999 for 7-seater Kia Carens EV and ₹5,200 for BYD E-MAX 7. All-inclusive with tolls, GST invoice provided.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">Do you pickup from Magarpatta and Kharadi?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Yes! We provide doorstep pickup from Hadapsar, Magarpatta City, Kharadi, EON IT Park, Wagholi, and all East Pune areas. Corporate pickups from IT parks available.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">Is GST invoice provided for corporate bookings?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Yes! We provide GST invoices for all bookings, making it easy for corporate expense reimbursement. We also offer monthly billing for companies with regular travel needs.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">How long from Hadapsar to Mumbai Airport?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Journey takes 2.5 to 3 hours from Hadapsar/Magarpatta to Mumbai Airport (160 km). We recommend leaving 5-6 hours before domestic flights and 6-7 hours before international.
            </p>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- CTA -->
    <div class="text-center bg-primary/5 rounded-lg p-8 md:p-12">
      <h2 class="text-2xl md:text-3xl font-bold mb-4">
        Book Your Hadapsar to Mumbai Airport Electric SUV Cab
      </h2>
      <p class="text-muted-foreground mb-8 text-lg">
        GST Invoice • Zero surge • 24/7 availability • Corporate packages • ₹700 cheaper
      </p>
      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <a href="https://wa.me/917498264215?text=Hi%2C%20I%20want%20to%20book%20electric%20SUV%20from%20Hadapsar%20to%20Mumbai%20Airport%0APickup%3A%20Hadapsar%2FMagarpatta%0ADate%3A%20%0ATime%3A%20%0APassengers%3A%20" target="_blank">
