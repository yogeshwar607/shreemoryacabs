---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Button } from "@/components/ui/button";
import Breadcrumbs from "@/components/seo/Breadcrumbs.astro";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";
import { MapPin, Clock, Check, Phone, MessageCircle, IndianRupee, Navigation, Star, Zap } from "lucide-react";

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Routes", url: "/routes" },
  { name: "Kothrud to Mumbai Airport", url: "/routes/kothrud-to-mumbai-airport" }
]);

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "name": "Kothrud to Mumbai Airport Electric SUV Cab Service",
  "serviceType": "Airport Transfer Service",
  "provider": {
    "@type": "Organization",
    "name": "GreenEVCabs",
    "telephone": "+91-7498264215"
  },
  "areaServed": [
    {
      "@type": "Place",
      "name": "Kothrud, Pune"
    },
    {
      "@type": "Airport",
      "name": "Mumbai Airport",
      "iataCode": "BOM"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2999",
    "priceCurrency": "INR"
  }
};
---

<MainLayout
  title="Kothrud to Mumbai Airport Electric SUV Cab ₹2,999 | 7 Seater Taxi"
  description="Book 7-seater electric SUV cab from Kothrud to Mumbai Airport at ₹2,999. Pickup from Kothrud, Karve Nagar, DP Road areas. Zero surge, flight tracking, 24/7 service. Save ₹700."
  keywords={[
    "kothrud to mumbai airport cab",
    "kothrud mumbai airport taxi",
    "electric suv cab kothrud to mumbai airport",
    "7 seater cab kothrud mumbai airport",
    "kothrud to bom airport cab",
    "karve nagar mumbai airport cab",
    "dp road to mumbai airport",
    "cheapest cab kothrud mumbai airport"
  ]}
>
  <Breadcrumbs items={[
    { name: "Home", href: "/" },
    { name: "Routes", href: "/routes" },
    { name: "Kothrud to Mumbai Airport", href: "/routes/kothrud-to-mumbai-airport" }
  ]} />

  <StructuredData data={[breadcrumbSchema, serviceSchema]} />

  <div class="container px-4 py-12 md:py-16">
    <!-- Hero -->
    <div class="text-center mb-12">
      <Badge variant="secondary" className="mb-4">
        Kothrud Pickup • Zero Surge • 24/7 Available • Flight Tracking
      </Badge>
      <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold tracking-tight mb-6">
        Kothrud to Mumbai Airport Electric SUV Cab ₹2,999
      </h1>
      <p class="text-xl md:text-2xl text-muted-foreground max-w-4xl mx-auto leading-relaxed mb-8">
        Book premium 7-seater electric SUV cab from Kothrud to Mumbai Airport (BOM) at just ₹2,999. Pickup from Kothrud, Karve Nagar, DP Road, Law College Road areas. Spacious Kia Carens EV & BYD E-MAX 7. Zero surge, all tolls included.
      </p>

      <!-- Trust Signals -->
      <div class="flex flex-wrap justify-center gap-6 mb-8">
        <div class="flex items-center gap-2">
          <Star className="h-5 w-5 text-yellow-500 fill-yellow-500" />
          <span class="font-semibold">4.9/5 Rating</span>
        </div>
        <div class="flex items-center gap-2">
          <Zap className="h-5 w-5 text-green-600" />
          <span class="font-semibold">100% Electric</span>
        </div>
        <div class="flex items-center gap-2">
          <Check className="h-5 w-5 text-primary" />
          <span class="font-semibold">₹700 Cheaper</span>
        </div>
      </div>

      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <a href="https://wa.me/917498264215?text=Hi%2C%20I%20want%20to%20book%20electric%20SUV%20cab%20from%20Kothrud%20to%20Mumbai%20Airport%0APickup%20Location%3A%20Kothrud%0ADate%20%26%20Time%3A%20%0APassengers%3A%20" target="_blank">
          <Button size="lg" className="bg-green-500 hover:bg-green-600 text-white">
            <MessageCircle className="mr-2 h-5 w-5" />
            Book on WhatsApp Now
          </Button>
        </a>
        <a href="tel:+917498264215">
          <Button size="lg" variant="outline">
            <Phone className="mr-2 h-5 w-5" />
            Call +91 74982 64215
          </Button>
        </a>
      </div>
    </div>

    <!-- Route Info -->
    <div class="mb-16 bg-primary/5 rounded-lg p-8">
      <div class="grid md:grid-cols-4 gap-6 text-center">
        <div>
          <MapPin className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Pickup Area</h3>
          <p class="text-2xl font-bold text-primary">Kothrud</p>
          <p class="text-sm text-muted-foreground">Central Pune</p>
        </div>
        <div>
          <Navigation className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Distance</h3>
          <p class="text-2xl font-bold text-primary">148 km</p>
          <p class="text-sm text-muted-foreground">Via Expressway</p>
        </div>
        <div>
          <Clock className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Journey Time</h3>
          <p class="text-2xl font-bold text-primary">2.5-3 hrs</p>
          <p class="text-sm text-muted-foreground">Traffic dependent</p>
        </div>
        <div>
          <IndianRupee className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Fixed Fare</h3>
          <p class="text-2xl font-bold text-primary">₹2,999</p>
          <p class="text-sm text-muted-foreground">Tolls included</p>
        </div>
      </div>
    </div>

    <!-- Kothrud Pickup Locations -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Pickup from All Kothrud Areas</h2>
      <p class="text-center text-muted-foreground mb-8 max-w-3xl mx-auto">
        We provide doorstep pickup from all residential and commercial areas in and around Kothrud
      </p>

      <div class="grid md:grid-cols-3 gap-6 max-w-5xl mx-auto">
        <Card>
          <CardHeader>
            <CardTitle>Kothrud Main Area</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Kothrud Depot</li>
              <li>• DP Road</li>
              <li>• Paud Road</li>
              <li>• Kothrud Gaothan</li>
              <li>• MIT College area</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Karve Nagar & Warje</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Karve Nagar</li>
              <li>• Warje</li>
              <li>• Karve Road</li>
              <li>• Ideal Colony</li>
              <li>• Mayur Colony</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Nearby Areas</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Law College Road</li>
              <li>• Erandwane</li>
              <li>• Shivajinagar proximity</li>
              <li>• Deccan proximity</li>
              <li>• Parvati area</li>
            </ul>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Pricing -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Kothrud to Mumbai Airport Cab Fare</h2>

      <div class="grid md:grid-cols-2 gap-6 max-w-4xl mx-auto">
        <Card className="border-2 border-primary">
          <CardHeader className="bg-primary/5">
            <CardTitle class="flex items-center justify-between">
              Kia Carens EV (7-Seater SUV)
              <Badge>Best Value</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹2,999</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Spacious 7-seater electric SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">All tolls & parking included</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Zero surge pricing - 24/7 same rate</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Flight tracking included</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20Kia%20Carens%20Kothrud%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full">Book Now ₹2,999</Button>
            </a>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="flex items-center justify-between">
              BYD E-MAX 7 (Premium SUV)
              <Badge variant="secondary">Luxury</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹5,200</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Premium 7-seater luxury SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">530 km extended range</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Captain seats & extra legroom</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Advanced comfort features</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20BYD%20E-MAX%207%20Kothrud%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full" variant="outline">Book Premium SUV</Button>
            </a>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Why Choose -->
    <div class="mb-16 bg-muted/50 rounded-lg p-8">
      <h2 class="text-3xl font-bold mb-6 text-center">Why Choose Electric SUV Cab from Kothrud?</h2>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Central Kothrud Location
          </h3>
          <p class="text-sm text-muted-foreground">
            Easy pickup from central Pune location. Quick access to Expressway via Chandani Chowk or Katraj routes.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            7-Seater Family SUVs
          </h3>
          <p class="text-sm text-muted-foreground">
            Perfect for family airport transfers. Comfortable seating for 6-7 people with space for 4-5 large suitcases.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Save ₹700 Per Trip
          </h3>
          <p class="text-sm text-muted-foreground">
            ₹2,999 vs ₹3,700 charged by other electric cab services. Best rates for premium SUV airport transfers.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Round-the-Clock Service
          </h3>
          <p class="text-sm text-muted-foreground">
            24/7 availability including early morning 3 AM, 4 AM pickups for morning flights. We never cancel.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            100% Electric Zero Pollution
          </h3>
          <p class="text-sm text-muted-foreground">
            Travel guilt-free in completely electric vehicles. Silent operation, smooth ride, zero emissions.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
