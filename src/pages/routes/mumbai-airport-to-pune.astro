  { name: "Mumbai Airport to Pune", url: "/routes/mumbai-airport-to-pune" }
]);

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "@id": "https://greenevcabs.com/routes/mumbai-airport-to-pune#service",
  "name": "Mumbai Airport to Pune Electric Cab Pickup Service",
  "serviceType": "Airport Pickup Service",
  "provider": {
    "@type": "Organization",
    "name": "GreenEVCabs",
    "url": "https://greenevcabs.com",
    "logo": "https://greenevcabs.com/logo.png",
    "telephone": "+91-7498264215",
    "aggregateRating": {
      "@type": "AggregateRating",
      "ratingValue": "4.9",
      "reviewCount": "287"
    }
  },
  "areaServed": [
    {
      "@type": "Airport",
      "name": "Chhatrapati Shivaji Maharaj International Airport",
      "iataCode": "BOM"
    },
    {
      "@type": "City",
      "name": "Pune"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2999",
    "priceCurrency": "INR",
    "availability": "https://schema.org/InStock",
    "validFrom": "2025-01-01"
  }
};

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "How much does cab from Mumbai Airport to Pune cost?",
