const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "How much does electric cab from Pune to Navi Mumbai Airport cost?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Our electric cab from Pune to Navi Mumbai Airport costs ₹2,999 for Kia Carens EV (6-7 seater) and ₹5,200 for BYD E-MAX 7 (7 seater). This is an all-inclusive price with zero surge pricing, tolls included, and no hidden charges."
      }
    },
    {
      "@type": "Question",
      "name": "How long does it take from Pune to Navi Mumbai Airport by cab?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "The journey from Pune to Navi Mumbai Airport takes approximately 2 to 2.5 hours (120 km). Travel time may vary based on traffic conditions. The new airport is more accessible from Pune than Mumbai Airport, saving you travel time."
      }
    },
    {
      "@type": "Question",
      "name": "Do you provide early morning pickup for flights from Navi Mumbai Airport?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes! We operate 24/7 with guaranteed availability for early morning pickups. Whether your flight is at 3 AM, 4 AM, 5 AM or any time, we provide reliable electric cab service from Pune to Navi Mumbai Airport. Book at least 24 hours in advance for early morning trips."
      }
    },
    {
      "@type": "Question",
      "name": "Is toll charge included in Pune to Navi Mumbai Airport cab fare?",
