---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Button } from "@/components/ui/button";
import Breadcrumbs from "@/components/seo/Breadcrumbs.astro";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";
import { MapPin, Clock, Check, Phone, MessageCircle, IndianRupee, Navigation, Star, Zap } from "lucide-react";

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Routes", url: "/routes" },
  { name: "Wakad to Mumbai Airport", url: "/routes/wakad-to-mumbai-airport" }
]);

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "name": "Wakad to Mumbai Airport Electric SUV Cab Service",
  "serviceType": "Airport Transfer Service",
  "provider": {
    "@type": "Organization",
    "name": "GreenEVCabs",
    "telephone": "+91-7498264215"
  },
  "areaServed": [
    {
      "@type": "Place",
      "name": "Wakad, Pune"
    },
    {
      "@type": "Airport",
      "name": "Mumbai Airport",
      "iataCode": "BOM"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2999",
    "priceCurrency": "INR"
  }
};
---

<MainLayout
  title="Wakad to Mumbai Airport Electric SUV Cab ₹2,999 | 7 Seater EV Taxi"
  description="Book 7-seater electric SUV cab from Wakad to Mumbai Airport at ₹2,999. Pickup from Wakad, Dange Chowk, Pimple Saudagar areas. Zero surge, flight tracking, 24/7 service. Save ₹700 vs competitors."
  keywords={[
    "wakad to mumbai airport cab",
    "wakad mumbai airport taxi",
    "electric suv cab wakad to mumbai airport",
    "7 seater cab wakad mumbai airport",
    "wakad to bom airport cab",
    "dange chowk mumbai airport cab",
    "pimple saudagar to mumbai airport",
    "cheapest cab wakad mumbai airport"
  ]}
>
  <Breadcrumbs items={[
    { name: "Home", href: "/" },
    { name: "Routes", href: "/routes" },
    { name: "Wakad to Mumbai Airport", href: "/routes/wakad-to-mumbai-airport" }
  ]} />

  <StructuredData data={[breadcrumbSchema, serviceSchema]} />

  <div class="container px-4 py-12 md:py-16">
    <!-- Hero -->
    <div class="text-center mb-12">
      <Badge variant="secondary" className="mb-4">
        Wakad Pickup • Zero Surge • 24/7 Available • Flight Tracking
      </Badge>
      <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold tracking-tight mb-6">
        Wakad to Mumbai Airport Electric SUV Cab ₹2,999
      </h1>
      <p class="text-xl md:text-2xl text-muted-foreground max-w-4xl mx-auto leading-relaxed mb-8">
        Book premium 7-seater electric SUV cab from Wakad to Mumbai Airport (BOM) at just ₹2,999. Pickup from Wakad, Dange Chowk, Pimple Saudagar, Pimple Nilakh areas. Spacious Kia Carens EV & BYD E-MAX 7. Zero surge pricing, all tolls included.
      </p>

      <!-- Trust Signals -->
      <div class="flex flex-wrap justify-center gap-6 mb-8">
        <div class="flex items-center gap-2">
          <Star className="h-5 w-5 text-yellow-500 fill-yellow-500" />
          <span class="font-semibold">4.9/5 Rating</span>
        </div>
        <div class="flex items-center gap-2">
          <Zap className="h-5 w-5 text-green-600" />
          <span class="font-semibold">100% Electric</span>
        </div>
        <div class="flex items-center gap-2">
          <Check className="h-5 w-5 text-primary" />
          <span class="font-semibold">₹700 Cheaper</span>
        </div>
      </div>

      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <a href="https://wa.me/917498264215?text=Hi%2C%20I%20want%20to%20book%20electric%20SUV%20cab%20from%20Wakad%20to%20Mumbai%20Airport%0APickup%20Location%3A%20Wakad%0ADate%20%26%20Time%3A%20%0APassengers%3A%20" target="_blank">
          <Button size="lg" className="bg-green-500 hover:bg-green-600 text-white">
            <MessageCircle className="mr-2 h-5 w-5" />
            Book on WhatsApp Now
          </Button>
        </a>
        <a href="tel:+917498264215">
          <Button size="lg" variant="outline">
            <Phone className="mr-2 h-5 w-5" />
            Call +91 74982 64215
          </Button>
        </a>
      </div>
    </div>

    <!-- Route Info -->
    <div class="mb-16 bg-primary/5 rounded-lg p-8">
      <div class="grid md:grid-cols-4 gap-6 text-center">
        <div>
          <MapPin className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Pickup Area</h3>
          <p class="text-2xl font-bold text-primary">Wakad</p>
          <p class="text-sm text-muted-foreground">West Pune</p>
        </div>
        <div>
          <Navigation className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Distance</h3>
          <p class="text-2xl font-bold text-primary">145 km</p>
          <p class="text-sm text-muted-foreground">Via Expressway</p>
        </div>
        <div>
          <Clock className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Journey Time</h3>
          <p class="text-2xl font-bold text-primary">2.5-3 hrs</p>
          <p class="text-sm text-muted-foreground">Traffic dependent</p>
        </div>
        <div>
          <IndianRupee className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Fixed Fare</h3>
          <p class="text-2xl font-bold text-primary">₹2,999</p>
          <p class="text-sm text-muted-foreground">Tolls included</p>
        </div>
      </div>
    </div>

    <!-- Wakad Pickup Locations -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Pickup from All Wakad Areas</h2>
      <p class="text-center text-muted-foreground mb-8 max-w-3xl mx-auto">
        We provide doorstep pickup from all residential and commercial areas in and around Wakad
      </p>

      <div class="grid md:grid-cols-3 gap-6 max-w-5xl mx-auto">
        <Card>
          <CardHeader>
            <CardTitle>Wakad Main Area</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Wakad Chowk area</li>
              <li>• Shankar Kalat Nagar</li>
              <li>• Wakad IT Parks</li>
              <li>• Wakad Society areas</li>
              <li>• MIDC Wakad</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Dange Chowk Area</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Dange Chowk Main Road</li>
              <li>• Thergaon</li>
              <li>• Kalewadi</li>
              <li>• Kasarwadi proximity</li>
              <li>• Residential societies</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Nearby Areas</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Pimple Saudagar</li>
              <li>• Pimple Nilakh</li>
              <li>• Rahatani</li>
              <li>• Pimpri</li>
              <li>• Vishal Nagar</li>
            </ul>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Pricing -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Wakad to Mumbai Airport Cab Fare</h2>

      <div class="grid md:grid-cols-2 gap-6 max-w-4xl mx-auto">
        <Card className="border-2 border-primary">
          <CardHeader className="bg-primary/5">
            <CardTitle class="flex items-center justify-between">
              Kia Carens EV (7-Seater SUV)
              <Badge>Best Value</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹2,999</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Spacious 7-seater electric SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">All tolls & parking included</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Zero surge pricing - 24/7 same rate</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Flight tracking included</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20Kia%20Carens%20Wakad%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full">Book Now ₹2,999</Button>
            </a>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="flex items-center justify-between">
              BYD E-MAX 7 (Premium SUV)
              <Badge variant="secondary">Luxury</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹5,200</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Premium 7-seater luxury SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">530 km extended range</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Captain seats & extra legroom</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Advanced comfort features</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20BYD%20E-MAX%207%20Wakad%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full" variant="outline">Book Premium SUV</Button>
            </a>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Why Choose -->
    <div class="mb-16 bg-muted/50 rounded-lg p-8">
      <h2 class="text-3xl font-bold mb-6 text-center">Why Choose Electric SUV Cab from Wakad?</h2>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Convenient Wakad Pickup
          </h3>
          <p class="text-sm text-muted-foreground">
            Door-to-door service from any location in Wakad, Dange Chowk, or Pimple Saudagar. No need to travel to city center for airport pickup.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Spacious 7-Seater SUVs
          </h3>
          <p class="text-sm text-muted-foreground">
            Perfect for families and groups. Ample luggage space for 4-5 large suitcases. Travel comfortably with everyone together.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            ₹700 Cheaper than Competitors
          </h3>
          <p class="text-sm text-muted-foreground">
            Same premium electric SUVs at ₹2,999 vs ₹3,700 charged by other services. Save money without compromising quality.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Early Morning Availability
          </h3>
          <p class="text-sm text-muted-foreground">
            Need pickup at 3 AM or 4 AM? We operate 24/7 with guaranteed availability for all flight timings from Wakad.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Zero Emissions Travel
          </h3>
          <p class="text-sm text-muted-foreground">
            100% electric vehicles with zero tailpipe emissions. Silent, smooth journey while reducing your carbon footprint.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            No Surge Pricing Ever
          </h3>
          <p class="text-sm text-muted-foreground">
            ₹2,999 at 2 AM or 2 PM - same fixed rate always. No peak hour charges, no weekend premiums, no hidden fees.
          </p>
        </div>
      </div>
    </div>

    <!-- FAQ -->
    <div class="mb-16">
      <h2 class="text-3xl font-bold mb-8 text-center">FAQs - Wakad to Mumbai Airport Cab</h2>

      <div class="space-y-4 max-w-4xl mx-auto">
        <Card>
          <CardHeader>
            <CardTitle class="text-lg">How much does cab from Wakad to Mumbai Airport cost?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Our electric SUV cab from Wakad to Mumbai Airport costs ₹2,999 for the spacious 7-seater Kia Carens EV, and ₹5,200 for the premium BYD E-MAX 7. This is all-inclusive pricing with tolls, parking, and zero surge charges.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">Do you pickup from Dange Chowk and Pimple Saudagar?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Yes! We provide doorstep pickup from Wakad, Dange Chowk, Pimple Saudagar, Pimple Nilakh, Rahatani, and all surrounding areas. Just share your exact pickup address when booking.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">How long does it take from Wakad to Mumbai Airport?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              The journey takes approximately 2.5 to 3 hours from Wakad to Mumbai Airport via the Mumbai-Pune Expressway (145 km). We recommend leaving 5-6 hours before your flight for domestic and 6-7 hours for international flights.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">Can I book for early morning flights from Wakad?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Absolutely! We provide 24/7 service including early morning pickups from Wakad at 12 AM, 1 AM, 2 AM, 3 AM, 4 AM, or any time you need. Book in advance for guaranteed availability.
            </p>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- CTA -->
    <div class="text-center bg-primary/5 rounded-lg p-8 md:p-12">
      <h2 class="text-2xl md:text-3xl font-bold mb-4">
        Book Your Wakad to Mumbai Airport Electric SUV Cab
      </h2>
      <p class="text-muted-foreground mb-8 text-lg">
        Zero surge pricing • 24/7 availability • Spacious 7-seater SUVs • ₹700 cheaper than competitors
      </p>
      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <a href="https://wa.me/917498264215?text=Hi%2C%20I%20want%20to%20book%20electric%20SUV%20from%20Wakad%20to%20Mumbai%20Airport%0APickup%3A%20Wakad%0ADate%3A%20%0ATime%3A%20%0APassengers%3A%20" target="_blank">
          <Button size="lg" className="bg-green-500 hover:bg-green-600 text-white">
            <MessageCircle className="mr-2 h-5 w-5" />
