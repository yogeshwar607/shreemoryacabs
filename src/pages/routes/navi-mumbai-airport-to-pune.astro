---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Button } from "@/components/ui/button";
import Breadcrumbs from "@/components/seo/Breadcrumbs.astro";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";
import { Plane, MapPin, Clock, Check, Zap, Shield, Phone, MessageCircle, Users, Star, Leaf, Navigation, IndianRupee, UserCheck } from "lucide-react";

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Routes", url: "/routes" },
  { name: "Navi Mumbai Airport to Pune", url: "/routes/navi-mumbai-airport-to-pune" }
]);

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "@id": "https://greenevcabs.com/routes/navi-mumbai-airport-to-pune#service",
  "name": "Navi Mumbai Airport to Pune Electric Cab Pickup Service",
  "serviceType": "Airport Pickup Service",
  "provider": {
    "@type": "Organization",
    "name": "GreenEVCabs",
    "url": "https://greenevcabs.com",
    "logo": "https://greenevcabs.com/logo.png",
    "telephone": "+91-7498264215",
    "aggregateRating": {
      "@type": "AggregateRating",
      "ratingValue": "4.9",
      "reviewCount": "287"
    }
  },
  "areaServed": [
    {
      "@type": "Airport",
      "name": "Navi Mumbai International Airport",
      "iataCode": "NMI"
    },
    {
      "@type": "City",
      "name": "Pune"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2999",
    "priceCurrency": "INR",
    "availability": "https://schema.org/InStock",
    "validFrom": "2025-01-01"
  },
  "hoursAvailable": {
    "@type": "OpeningHoursSpecification",
    "dayOfWeek": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"],
    "opens": "00:00",
    "closes": "23:59"
  }
};

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "How much does cab from Navi Mumbai Airport to Pune cost?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Electric cab from Navi Mumbai Airport to Pune costs ₹2,999 for Kia Carens EV and ₹5,200 for BYD E-MAX 7. Price includes tolls, parking, and waiting time for flight delays. No surge pricing, fixed rates 24/7."
      }
    },
    {
      "@type": "Question",
      "name": "Do you charge extra if my flight is delayed?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "No! Flight delays are tracked automatically and there's absolutely no waiting charge. Whether your flight is 30 minutes or 3 hours late, the price remains ₹2,999. This is our commitment to hassle-free airport pickup service."
      }
    },
    {
      "@type": "Question",
      "name": "How do I find my driver at Navi Mumbai Airport?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "We provide meet & greet service with name board at the arrivals area. Our driver will track your flight and be waiting for you at the designated pickup point. You'll receive driver details (name, photo, vehicle number) via WhatsApp 2 hours before your scheduled arrival."
      }
    }
  ]
};
---

<MainLayout
  title="Navi Mumbai Airport to Pune Cab ₹2,999 | NMI Electric Taxi Pickup | Flight Tracking"
  description="Book electric cab pickup from Navi Mumbai Airport (NMI) to Pune at ₹2,999. Flight tracking, meet & greet, name board service. Zero surge, tolls included. 24/7 availability."
  keywords={[
    "navi mumbai airport to pune cab",
    "nmi to pune taxi",
    "navi mumbai airport pickup",
    "electric cab nmi to pune",
    "navi mumbai airport transfer pune",
    "cab from navi mumbai airport",
    "nmi airport taxi service"
  ]}
>
  <Breadcrumbs items={[
    { name: "Home", href: "/" },
    { name: "Routes", href: "/routes" },
    { name: "Navi Mumbai Airport to Pune", href: "/routes/navi-mumbai-airport-to-pune" }
  ]} />

  <StructuredData data={[breadcrumbSchema, serviceSchema, faqSchema]} />

  <div class="container px-4 py-12 md:py-16">
    <!-- Hero Section -->
    <div class="text-center mb-12">
      <Badge variant="secondary" className="mb-4">
        Flight Tracking • Meet & Greet • Zero Waiting Charges
      </Badge>
      <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold tracking-tight mb-6">
        Navi Mumbai Airport to Pune Electric Cab Pickup | Flight Tracking ₹2,999
      </h1>
      <p class="text-xl md:text-2xl text-muted-foreground max-w-4xl mx-auto leading-relaxed mb-8">
        Reliable electric cab pickup from Navi Mumbai International Airport (NMI) to anywhere in Pune at ₹2,999.
        Real-time flight tracking, meet & greet with name board, zero waiting charges for delays. 24/7 service.
      </p>

      <!-- Key Features -->
      <div class="grid grid-cols-1 md:grid-cols-3 gap-4 max-w-3xl mx-auto mb-8">
        <div class="flex items-center justify-center gap-2 p-4 bg-muted/50 rounded-lg">
          <UserCheck className="h-5 w-5 text-primary" />
          <span class="font-medium">Meet & Greet Service</span>
        </div>
        <div class="flex items-center justify-center gap-2 p-4 bg-muted/50 rounded-lg">
          <Plane className="h-5 w-5 text-primary" />
          <span class="font-medium">Flight Tracking</span>
        </div>
        <div class="flex items-center justify-center gap-2 p-4 bg-muted/50 rounded-lg">
          <Clock className="h-5 w-5 text-primary" />
          <span class="font-medium">Zero Delay Charges</span>
        </div>
      </div>

      <!-- CTA Buttons -->
      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <a href="https://wa.me/917498264215?text=Book%20Navi%20Mumbai%20Airport%20to%20Pune%20Pickup" target="_blank" rel="noopener noreferrer">
          <Button size="lg" className="w-full sm:w-auto bg-green-600 hover:bg-green-700">
            <MessageCircle className="mr-2 h-5 w-5" />
            Book Airport Pickup
          </Button>
        </a>
        <a href="tel:+917498264215">
          <Button size="lg" variant="outline" className="w-full sm:w-auto">
            <Phone className="mr-2 h-5 w-5" />
            Call: +91 74982 64215
          </Button>
        </a>
      </div>

      <!-- Price Display -->
      <Card className="max-w-md mx-auto mt-8">
        <CardContent className="pt-6">
          <div class="text-center">
            <p class="text-sm text-muted-foreground mb-2">Fixed Price - No Surge</p>
            <div class="flex items-center justify-center gap-2 mb-2">
              <IndianRupee className="h-6 w-6 text-primary" />
              <span class="text-5xl font-bold text-primary">2,999</span>
            </div>
            <p class="text-sm text-muted-foreground">₹2,999 all-inclusive, no surge pricing</p>
          </div>
        </CardContent>
      </Card>
    </div>

    <!-- How It Works Section -->
    <section class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold text-center mb-12">
        How Our Airport Pickup Service Works
      </h2>
      <div class="grid md:grid-cols-4 gap-6">
        <Card>
          <CardHeader>
            <div class="bg-primary text-primary-foreground w-12 h-12 rounded-full flex items-center justify-center text-xl font-bold mb-4">
              1
            </div>
            <CardTitle>Book in Advance</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Book via WhatsApp or call with your flight details. We recommend booking 24 hours before arrival.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <div class="bg-primary text-primary-foreground w-12 h-12 rounded-full flex items-center justify-center text-xl font-bold mb-4">
              2
            </div>
            <CardTitle>We Track Your Flight</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Our system automatically tracks your flight in real-time. Delayed? No problem - we adjust pickup time.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <div class="bg-primary text-primary-foreground w-12 h-12 rounded-full flex items-center justify-center text-xl font-bold mb-4">
              3
            </div>
            <CardTitle>Meet & Greet</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Driver waits at arrivals with name board. You'll receive driver details 2 hours before landing.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <div class="bg-primary text-primary-foreground w-12 h-12 rounded-full flex items-center justify-center text-xl font-bold mb-4">
              4
            </div>
            <CardTitle>Comfortable Journey</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Relax in our premium electric vehicle and reach your Pune destination in comfort.
            </p>
          </CardContent>
        </Card>
      </div>
    </section>

    <!-- Benefits Section -->
    <section class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold text-center mb-12">
        Why Choose Our Airport Pickup Service?
      </h2>
      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
        <Card>
          <CardHeader>
            <Plane className="h-10 w-10 text-primary mb-2" />
            <CardTitle>Real-Time Flight Tracking</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Automatic flight tracking means your driver knows if you're early or delayed. Zero stress, zero waiting charges.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <UserCheck className="h-10 w-10 text-primary mb-2" />
            <CardTitle>Meet & Greet with Name Board</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Driver waits at arrivals area with your name board. Easy to spot, professional service.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <Clock className="h-10 w-10 text-primary mb-2" />
            <CardTitle>Zero Waiting Charges</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Flight delayed 3 hours? Price remains ₹2,999. We don't charge extra for circumstances beyond your control.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <Leaf className="h-10 w-10 text-primary mb-2" />
            <CardTitle>Eco-Friendly Travel</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              100% electric vehicles mean zero emissions. Start your Pune journey the sustainable way.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <Shield className="h-10 w-10 text-primary mb-2" />
            <CardTitle>24/7 Availability</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Late night arrival? Early morning flight? We operate 24/7 with professional, verified drivers.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <Users className="h-10 w-10 text-primary mb-2" />
            <CardTitle>Spacious & Comfortable</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              6-7 seater vehicles with ample luggage space. Perfect for families and business travelers.
            </p>
          </CardContent>
        </Card>
      </div>
    </section>

    <!-- Pricing Section -->
    <section class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold text-center mb-8">
        Transparent Pricing - No Hidden Charges
      </h2>
      <div class="grid md:grid-cols-2 gap-8 max-w-4xl mx-auto">
        <Card className="border-2">
          <CardHeader>
            <div class="flex justify-between items-start">
              <div>
                <CardTitle className="text-2xl mb-2">Kia Carens EV</CardTitle>
                <p class="text-muted-foreground">6-7 Seater • 400+ km Range</p>
              </div>
              <Badge variant="secondary">Popular</Badge>
            </div>
          </CardHeader>
          <CardContent>
            <div class="text-4xl font-bold text-primary mb-4">₹2,999</div>
            <p class="text-sm text-muted-foreground mb-6">All-inclusive fixed rate</p>

            <div class="space-y-3 mb-6">
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Base fare</span>
                <span class="font-semibold text-primary">₹2,999</span>
              </div>
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Tolls & parking</span>
                <span class="font-semibold text-primary">₹0</span>
              </div>
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Waiting charges (flight delay)</span>
                <span class="font-semibold text-primary">₹0</span>
              </div>
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Meet & greet service</span>
                <span class="font-semibold text-primary">₹0</span>
              </div>
              <div class="border-t pt-3 flex justify-between font-bold">
                <span>Total (All-inclusive)</span>
                <span class="font-semibold text-primary">₹2,999</span>
              </div>
            </div>

            <a href="https://wa.me/917498264215?text=Book%20Airport%20Pickup%20NMI%20to%20Pune" target="_blank" rel="noopener noreferrer">
              <Button className="w-full bg-primary">Book Now ₹2,999</Button>
            </a>
          </CardContent>
        </Card>

        <Card className="border-2 border-primary">
          <CardHeader>
            <div class="flex justify-between items-start">
              <div>
                <CardTitle className="text-2xl mb-2">BYD E-MAX 7</CardTitle>
                <p class="text-muted-foreground">7 Seater • 530+ km Range</p>
              </div>
              <Badge>Premium</Badge>
            </div>
          </CardHeader>
          <CardContent>
            <div class="text-4xl font-bold text-primary mb-4">₹5,200</div>
            <p class="text-sm text-muted-foreground mb-6">Luxury electric experience</p>

            <div class="space-y-3 mb-6">
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Base fare</span>
                <span class="font-semibold text-primary">₹5,200</span>
              </div>
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Tolls & parking</span>
                <span class="font-semibold text-primary">₹0</span>
              </div>
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Waiting charges (flight delay)</span>
                <span class="font-semibold text-primary">₹0</span>
              </div>
              <div class="flex justify-between text-sm">
                <span class="text-muted-foreground">Meet & greet service</span>
                <span class="font-semibold text-primary">₹0</span>
              </div>
              <div class="border-t pt-3 flex justify-between font-bold">
                <span>Total (All-inclusive)</span>
                <span class="font-semibold text-primary">₹5,200</span>
              </div>
            </div>

            <a href="https://wa.me/917498264215?text=Book%20BYD%20Airport%20Pickup%20NMI%20to%20Pune" target="_blank" rel="noopener noreferrer">
              <Button className="w-full bg-primary">Book BYD E-MAX 7 ₹5,200</Button>
            </a>
          </CardContent>
        </Card>
      </div>
    </section>

    <!-- FAQ Section -->
    <section class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold text-center mb-12">
        Frequently Asked Questions
      </h2>
      <div class="max-w-3xl mx-auto space-y-6">
        <Card>
          <CardHeader>
            <CardTitle>How much does cab from Navi Mumbai Airport to Pune cost?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Electric cab from Navi Mumbai Airport to Pune costs ₹2,999 for Kia Carens EV and ₹5,200 for BYD E-MAX 7. Price includes tolls, parking, and waiting time for flight delays. No surge pricing, fixed rates 24/7.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Do you charge extra if my flight is delayed?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              No! Flight delays are tracked automatically and there's absolutely no waiting charge. Whether your flight is 30 minutes or 3 hours late, the price remains ₹2,999. This is our commitment to hassle-free airport pickup service.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>How do I find my driver at Navi Mumbai Airport?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              We provide meet & greet service with name board at the arrivals area. Our driver will track your flight and be waiting for you at the designated pickup point. You'll receive driver details (name, photo, vehicle number) via WhatsApp 2 hours before your scheduled arrival.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>What information do I need to provide for airport pickup?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              We need: 1) Flight number and airline, 2) Arrival date and time, 3) Your name (for the name board), 4) Mobile number, 5) Drop location in Pune. That's it! We'll handle the rest including flight tracking.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Can I book for late night arrivals?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Absolutely! We operate 24/7. Late night and early morning pickups are available at the same price of ₹2,999. No extra charges for odd hours. Just book in advance and we'll be there.
            </p>
          </CardContent>
        </Card>
      </div>
    </section>

    <!-- Final CTA -->
    <section class="text-center">
      <Card className="max-w-2xl mx-auto bg-gradient-to-br from-primary/10 to-background">
        <CardContent className="pt-8 pb-8">
          <h2 class="text-3xl font-bold mb-4">
            Book Your Navi Mumbai Airport Pickup Now
          </h2>
          <p class="text-lg text-muted-foreground mb-6">
            Hassle-free airport pickup with flight tracking, meet & greet, and zero waiting charges
          </p>
          <div class="flex flex-col sm:flex-row gap-4 justify-center">
            <a href="https://wa.me/917498264215?text=Book%20NMI%20Airport%20Pickup" target="_blank" rel="noopener noreferrer">
              <Button size="lg" className="bg-green-600 hover:bg-green-700">
                <MessageCircle className="mr-2 h-5 w-5" />
                WhatsApp Booking
              </Button>
            </a>
            <a href="tel:+917498264215">
              <Button size="lg" variant="outline">
                <Phone className="mr-2 h-5 w-5" />
                Call Now
              </Button>
            </a>
          </div>
        </CardContent>
      </Card>
    </section>
  </div>
</MainLayout>
