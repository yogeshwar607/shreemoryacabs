---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Button } from "@/components/ui/button";
import Breadcrumbs from "@/components/seo/Breadcrumbs.astro";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";
import { MapPin, Clock, Check, Phone, MessageCircle, IndianRupee, Navigation, Star, Zap } from "lucide-react";

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Routes", url: "/routes" },
  { name: "Baner to Mumbai Airport", url: "/routes/baner-to-mumbai-airport" }
]);

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "name": "Baner to Mumbai Airport Electric SUV Cab Service",
  "serviceType": "Airport Transfer Service",
  "provider": {
    "@type": "Organization",
    "name": "GreenEVCabs",
    "telephone": "+91-7498264215"
  },
  "areaServed": [
    {
      "@type": "Place",
      "name": "Baner, Pune"
    },
    {
      "@type": "Airport",
      "name": "Mumbai Airport",
      "iataCode": "BOM"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2999",
    "priceCurrency": "INR"
  }
};
---

<MainLayout
  title="Baner to Mumbai Airport Electric SUV Cab ₹2,999 | 7 Seater EV Taxi"
  description="Book 7-seater electric SUV cab from Baner to Mumbai Airport at ₹2,999. Pickup from Baner, Balewadi, Pashan Link Road areas. Zero surge, flight tracking, 24/7. Save ₹700 vs competitors."
  keywords={[
    "baner to mumbai airport cab",
    "baner mumbai airport taxi",
    "electric suv cab baner to mumbai airport",
    "7 seater cab baner mumbai airport",
    "baner to bom airport cab",
    "balewadi mumbai airport cab",
    "pashan link road to mumbai airport",
    "cheapest cab baner mumbai airport"
  ]}
>
  <Breadcrumbs items={[
    { name: "Home", href: "/" },
    { name: "Routes", href: "/routes" },
    { name: "Baner to Mumbai Airport", href: "/routes/baner-to-mumbai-airport" }
  ]} />

  <StructuredData data={[breadcrumbSchema, serviceSchema]} />

  <div class="container px-4 py-12 md:py-16">
    <!-- Hero -->
    <div class="text-center mb-12">
      <Badge variant="secondary" className="mb-4">
        Baner Pickup • Zero Surge • 24/7 Available • Flight Tracking
      </Badge>
      <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold tracking-tight mb-6">
        Baner to Mumbai Airport Electric SUV Cab ₹2,999
      </h1>
      <p class="text-xl md:text-2xl text-muted-foreground max-w-4xl mx-auto leading-relaxed mb-8">
        Book premium 7-seater electric SUV cab from Baner to Mumbai Airport (BOM) at just ₹2,999. Pickup from Baner, Balewadi, Pashan Link Road, Aundh Road areas. Spacious Kia Carens EV & BYD E-MAX 7. Zero surge pricing, all tolls included.
      </p>

      <!-- Trust Signals -->
      <div class="flex flex-wrap justify-center gap-6 mb-8">
        <div class="flex items-center gap-2">
          <Star className="h-5 w-5 text-yellow-500 fill-yellow-500" />
          <span class="font-semibold">4.9/5 Rating</span>
        </div>
        <div class="flex items-center gap-2">
          <Zap className="h-5 w-5 text-green-600" />
          <span class="font-semibold">100% Electric</span>
        </div>
        <div class="flex items-center gap-2">
          <Check className="h-5 w-5 text-primary" />
          <span class="font-semibold">₹700 Cheaper</span>
        </div>
      </div>

      <div class="flex flex-col sm:flex-row gap-4 justify-center">
        <a href="https://wa.me/917498264215?text=Hi%2C%20I%20want%20to%20book%20electric%20SUV%20cab%20from%20Baner%20to%20Mumbai%20Airport%0APickup%20Location%3A%20Baner%0ADate%20%26%20Time%3A%20%0APassengers%3A%20" target="_blank">
          <Button size="lg" className="bg-green-500 hover:bg-green-600 text-white">
            <MessageCircle className="mr-2 h-5 w-5" />
            Book on WhatsApp Now
          </Button>
        </a>
        <a href="tel:+917498264215">
          <Button size="lg" variant="outline">
            <Phone className="mr-2 h-5 w-5" />
            Call +91 74982 64215
          </Button>
        </a>
      </div>
    </div>

    <!-- Route Info -->
    <div class="mb-16 bg-primary/5 rounded-lg p-8">
      <div class="grid md:grid-cols-4 gap-6 text-center">
        <div>
          <MapPin className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Pickup Area</h3>
          <p class="text-2xl font-bold text-primary">Baner</p>
          <p class="text-sm text-muted-foreground">West Pune</p>
        </div>
        <div>
          <Navigation className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Distance</h3>
          <p class="text-2xl font-bold text-primary">150 km</p>
          <p class="text-sm text-muted-foreground">Via Expressway</p>
        </div>
        <div>
          <Clock className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Journey Time</h3>
          <p class="text-2xl font-bold text-primary">2.5-3 hrs</p>
          <p class="text-sm text-muted-foreground">Traffic dependent</p>
        </div>
        <div>
          <IndianRupee className="h-10 w-10 text-primary mx-auto mb-3" />
          <h3 class="font-semibold mb-1">Fixed Fare</h3>
          <p class="text-2xl font-bold text-primary">₹2,999</p>
          <p class="text-sm text-muted-foreground">Tolls included</p>
        </div>
      </div>
    </div>

    <!-- Baner Pickup Locations -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Pickup from All Baner Areas</h2>
      <p class="text-center text-muted-foreground mb-8 max-w-3xl mx-auto">
        We provide doorstep pickup from all residential and commercial areas in and around Baner
      </p>

      <div class="grid md:grid-cols-3 gap-6 max-w-5xl mx-auto">
        <Card>
          <CardHeader>
            <CardTitle>Baner Main Area</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Baner Road</li>
              <li>• Baner Gaon</li>
              <li>• Baner Hill</li>
              <li>• Lunkad Colony</li>
              <li>• Residences & societies</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Pashan Link Road</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Baner-Pashan Link Road</li>
              <li>• IT Parks & Offices</li>
              <li>• Commercial complexes</li>
              <li>• SUS Road proximity</li>
              <li>• Premium apartments</li>
            </ul>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle>Nearby Areas</CardTitle>
          </CardHeader>
          <CardContent>
            <ul class="space-y-2 text-sm">
              <li>• Balewadi</li>
              <li>• Aundh Road</li>
              <li>• Pashan</li>
              <li>• Sus</li>
              <li>• Model Colony</li>
            </ul>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Pricing -->
    <div class="mb-16">
      <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">Baner to Mumbai Airport Cab Fare</h2>

      <div class="grid md:grid-cols-2 gap-6 max-w-4xl mx-auto">
        <Card className="border-2 border-primary">
          <CardHeader className="bg-primary/5">
            <CardTitle class="flex items-center justify-between">
              Kia Carens EV (7-Seater SUV)
              <Badge>Best Value</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹2,999</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Spacious 7-seater electric SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">All tolls & parking included</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Zero surge pricing - 24/7 same rate</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Flight tracking included</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20Kia%20Carens%20Baner%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full">Book Now ₹2,999</Button>
            </a>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="flex items-center justify-between">
              BYD E-MAX 7 (Premium SUV)
              <Badge variant="secondary">Luxury</Badge>
            </CardTitle>
          </CardHeader>
          <CardContent className="pt-6">
            <div class="mb-6">
              <div class="text-4xl font-bold text-primary mb-2">₹5,200</div>
              <p class="text-sm text-muted-foreground">All-inclusive one-way fare</p>
            </div>
            <ul class="space-y-3 mb-6">
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Premium 7-seater luxury SUV</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">530 km extended range</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Captain seats & extra legroom</span>
              </li>
              <li class="flex items-start gap-2">
                <Check className="h-5 w-5 text-primary mt-0.5 flex-shrink-0" />
                <span class="text-sm">Advanced comfort features</span>
              </li>
            </ul>
            <a href="https://wa.me/917498264215?text=Book%20BYD%20E-MAX%207%20Baner%20to%20Mumbai%20Airport" target="_blank">
              <Button className="w-full" variant="outline">Book Premium SUV</Button>
            </a>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- Why Choose -->
    <div class="mb-16 bg-muted/50 rounded-lg p-8">
      <h2 class="text-3xl font-bold mb-6 text-center">Why Choose Electric SUV Cab from Baner?</h2>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Convenient Baner Pickup
          </h3>
          <p class="text-sm text-muted-foreground">
            Door-to-door service from any location in Baner, Balewadi, or Pashan Link Road. No need to go to city center for airport cabs.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Perfect for Families
          </h3>
          <p class="text-sm text-muted-foreground">
            7-seater SUVs ideal for family trips. Spacious luggage capacity for 4-5 large suitcases plus cabin bags. Everyone travels comfortably.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Best Pricing Guarantee
          </h3>
          <p class="text-sm text-muted-foreground">
            ₹2,999 vs ₹3,700 charged by competitors. Save ₹700 on the same premium electric SUVs. No compromise on quality or service.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            24/7 Service
          </h3>
          <p class="text-sm text-muted-foreground">
            Early morning 3 AM or 4 AM flights? Late night arrivals? We operate round-the-clock with guaranteed cab availability from Baner.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Eco-Friendly Travel
          </h3>
          <p class="text-sm text-muted-foreground">
            100% electric vehicles produce zero emissions. Enjoy silent, smooth ride while contributing to cleaner air in Pune and Mumbai.
          </p>
        </div>

        <div>
          <h3 class="font-semibold mb-2 flex items-center gap-2">
            <Check className="h-5 w-5 text-green-600" />
            Fixed Pricing Always
          </h3>
          <p class="text-sm text-muted-foreground">
            ₹2,999 any time, any day. No surge pricing during peak hours, weekends, or holidays. All tolls and charges included.
          </p>
        </div>
      </div>
    </div>

    <!-- FAQ -->
    <div class="mb-16">
      <h2 class="text-3xl font-bold mb-8 text-center">FAQs - Baner to Mumbai Airport Cab</h2>

      <div class="space-y-4 max-w-4xl mx-auto">
        <Card>
          <CardHeader>
            <CardTitle class="text-lg">How much is cab fare from Baner to Mumbai Airport?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Electric SUV cab from Baner to Mumbai Airport costs ₹2,999 for 7-seater Kia Carens EV and ₹5,200 for premium BYD E-MAX 7. All-inclusive pricing with tolls, parking, and no surge charges.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">Do you pickup from Balewadi and Pashan Link Road?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Yes! We provide doorstep pickup from Baner, Balewadi, Pashan Link Road, Aundh Road, and all surrounding residential areas. Share your exact address during booking.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">How long does it take from Baner to Mumbai Airport?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              Journey takes approximately 2.5 to 3 hours from Baner to Mumbai Airport (150 km) via Expressway. We recommend leaving 5-6 hours before domestic flights and 6-7 hours before international flights.
            </p>
          </CardContent>
        </Card>

        <Card>
          <CardHeader>
            <CardTitle class="text-lg">Is 7-seater SUV necessary for airport transfer?</CardTitle>
          </CardHeader>
          <CardContent>
            <p class="text-muted-foreground">
              7-seater SUVs are perfect for families (4-6 people) with luggage, groups traveling together, or anyone wanting extra space and comfort. Our Kia Carens EV and BYD E-MAX 7 offer premium features at affordable rates.
            </p>
          </CardContent>
        </Card>
      </div>
    </div>

    <!-- CTA -->
    <div class="text-center bg-primary/5 rounded-lg p-8 md:p-12">
      <h2 class="text-2xl md:text-3xl font-bold mb-4">
