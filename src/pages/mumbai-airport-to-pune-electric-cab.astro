---
import MainLayout from "@/layouts/MainLayout.astro";
import { Button } from "@/components/ui/button";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Check, MapPin, Zap, IndianRupee, Clock, Plane, Shield, Users, Phone } from "lucide-react";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "serviceType": "Electric Cab Service Mumbai Airport to Pune",
  "provider": {
    "@type": "LocalBusiness",
    "name": "GreenEVCabs",
    "description": "Premium electric cab service from Mumbai Airport to Pune with flight tracking, 30 minutes free waiting, and professional drivers",
    "telephone": "+917498264215"
  },
  "areaServed": [
    {
      "@type": "City",
      "name": "Mumbai, Maharashtra, India"
    },
    {
      "@type": "City",
      "name": "Pune, Maharashtra, India"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "3700",
    "priceCurrency": "INR",
    "description": "All-inclusive fare with tolls, parking, driver allowance, and GST. Zero surge pricing.",
    "availability": "https://schema.org/InStock",
    "validFrom": "2025-01-01",
    "priceValidUntil": "2025-12-31"
  },
  "additionalProperty": [
    {
      "@type": "PropertyValue",
      "name": "Flight Tracking",
      "value": "Included"
    },
    {
      "@type": "PropertyValue",
      "name": "Free Waiting Time",
      "value": "30 minutes"
    },
    {
      "@type": "PropertyValue",
      "name": "Vehicle Type",
      "value": "Luxury 7-seater Electric SUV"
    }
  ]
};

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "How much does electric cab cost from Mumbai Airport to Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "GreenEVCabs charges ₹3,700 all-inclusive for Mumbai Airport to Pune in a luxury 7-seater electric SUV. This includes tolls (₹430), driver allowance (₹500), parking (₹70), and GST. No hidden fees, no surge pricing - same rate 24/7."
      }
    },
    {
      "@type": "Question",
      "name": "Which Mumbai Airport terminal do you pick up from?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "We pick up from all Mumbai Airport terminals: Terminal 1 (Domestic), Terminal 2 (International & Domestic), and Navi Mumbai Airport. Our drivers track your flight and meet you at arrivals with a name board."
      }
    },
    {
      "@type": "Question",
      "name": "Do you track my flight for pickup?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes! We track your flight in real-time. If your flight is delayed or arrives early, our driver adjusts pickup time automatically. No extra charges for flight delays. 30 minutes free waiting time after you land."
      }
    },
    {
      "@type": "Question",
      "name": "Is GreenEVCabs cheaper than other Mumbai Airport cabs?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, GreenEVCabs is ₹500-800 cheaper than other electric cab services (₹3,700 vs ₹4,200-4,500) and ₹1,300 cheaper than diesel cabs (₹5,000+). We offer direct booking without middleman commissions, plus zero surge pricing during peak hours."
      }
    },
    {
      "@type": "Question",
      "name": "How long does it take from Mumbai Airport to Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Mumbai Airport to Pune takes 2.5-3 hours (150 km) via Mumbai-Pune Expressway. Travel time varies based on traffic, your Pune destination, and airport exit time. Early morning (5-7 AM) is fastest, evening peak (6-9 PM) takes longer."
      }
    },
    {
      "@type": "Question",
      "name": "Can electric cabs handle Mumbai Airport to Pune distance?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Absolutely! Our Kia Carens EV and BYD eMax 7 have 400+ km range on full charge - nearly 3x the Mumbai-Pune distance (150 km). We maintain all vehicles with 80%+ battery before every trip. 99.8% on-time completion rate."
      }
    },
    {
      "@type": "Question",
      "name": "Do you provide service for early morning or late night flights?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, 24/7 service available! We handle red-eye flights (midnight-5 AM arrivals) and early morning departures. Book anytime via WhatsApp (+91 74982 64215). Our drivers are experienced with night highway travel and toll plaza operations."
      }
    },
    {
      "@type": "Question",
      "name": "Where exactly do you drop in Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "We drop anywhere in Pune at the same ₹3,700 rate: Hinjewadi, Wakad, Baner, Kothrud, Viman Nagar, Hadapsar, Kharadi, Magarpatta, Kore gaon Park, Deccan, Shivajinagar, Aundh, or any other location. No extra charges for specific Pune areas."
      }
    }
  ]
};

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Mumbai Airport to Pune Electric Cab", url: "/mumbai-airport-to-pune-electric-cab" }
]);
---

<MainLayout
  title="Mumbai Airport to Pune Electric Cab ₹3,700 | Flight Tracking | Free Waiting - GreenEVCabs"
  description="Mumbai Airport to Pune electric cab at ₹3,700 all-inclusive. Flight tracking, 30 min free waiting, pickup from T1/T2/Navi Mumbai Airport. Professional drivers, luxury 7-seater SUVs. Book 24/7."
  keywords={[
    "mumbai airport to pune cab",
    "mumbai airport to pune electric cab",
    "mumbai airport taxi to pune",
    "chhatrapati shivaji airport to pune cab",
    "mumbai terminal 2 to pune cab",
    "navi mumbai airport to pune cab",
    "mumbai airport pickup pune",
    "flight tracking cab mumbai pune"
  ]}
>
  <StructuredData data={[serviceSchema, faqSchema, breadcrumbSchema]} />

  <!-- Hero Section -->
  <section class="w-full py-12 md:py-20 bg-gradient-to-b from-primary/10 to-background">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto text-center">
        <Badge variant="secondary" className="mb-4">✈️ Airport Pickup Service</Badge>
        <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold tracking-tight mb-6">
          Mumbai Airport to Pune<br/>Electric Cab - ₹3,700
        </h1>
        <p class="text-xl md:text-2xl text-muted-foreground mb-4">
          Flight Tracking | 30 Minutes Free Waiting | All Terminals Pickup
        </p>
        <p class="text-lg text-muted-foreground mb-8 max-w-3xl mx-auto">
          Premium 7-seater electric SUVs from Mumbai Airport (T1, T2) & Navi Mumbai Airport to anywhere in Pune. Zero surge pricing. Professional drivers. Available 24/7.
        </p>

        <!-- Price Comparison -->
        <div class="bg-green-50 border-2 border-green-200 rounded-lg p-6 mb-8 max-w-2xl mx-auto">
          <h2 class="text-2xl font-bold mb-4">All-Inclusive Pricing</h2>
          <div class="grid md:grid-cols-3 gap-4 text-sm">
            <div class="bg-white rounded p-4">
              <div class="text-3xl font-bold text-green-600">₹3,700</div>
              <div class="text-muted-foreground">GreenEVCabs</div>
              <div class="text-xs mt-2">✓ All-inclusive</div>
            </div>
            <div class="bg-red-50 rounded p-4">
              <div class="text-3xl font-bold text-red-600">₹4,500+</div>
              <div class="text-muted-foreground">Other EV Cabs</div>
              <div class="text-xs mt-2">+ Surge charges</div>
            </div>
            <div class="bg-red-50 rounded p-4">
              <div class="text-3xl font-bold text-red-600">₹5,000+</div>
              <div class="text-muted-foreground">Diesel Cabs</div>
              <div class="text-xs mt-2">+ Multiple charges</div>
            </div>
          </div>
          <p class="text-sm mt-4 font-semibold text-green-700">Save ₹500-1,300 per trip with GreenEVCabs</p>
        </div>

        <!-- CTA Buttons -->
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href="https://wa.me/917498264215?text=Book%20Mumbai%20Airport%20to%20Pune%20Electric%20Cab" target="_blank">
            <Button size="lg" className="w-full sm:w-auto bg-green-600 hover:bg-green-700 text-lg px-8">
              <Phone class="w-5 h-5 mr-2" />
              Book on WhatsApp
            </Button>
          </a>
          <a href="tel:+917498264215">
            <Button size="lg" variant="outline" className="w-full sm:w-auto text-lg px-8">
              <Phone class="w-5 h-5 mr-2" />
              Call Now
            </Button>
          </a>
        </div>

        <p class="text-sm text-muted-foreground mt-4">
          ✓ Instant booking ✓ No advance payment ✓ Free cancellation (2+ hrs before)
        </p>
      </div>
    </div>
  </section>

  <!-- Key Features -->
  <section class="w-full py-12 md:py-16 bg-background">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto">
        <h2 class="text-3xl md:text-4xl font-bold text-center mb-12">Why Choose GreenEVCabs for Airport Pickup?</h2>

        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-6">
          <Card>
            <CardHeader>
              <Plane class="w-10 h-10 text-primary mb-2" />
              <CardTitle>Flight Tracking</CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-sm text-muted-foreground">
                We track your flight in real-time. Delayed? Early arrival? Driver adjusts automatically. No extra charges for delays.
              </p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <Clock class="w-10 h-10 text-primary mb-2" />
              <CardTitle>30 Min Free Wait</CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-sm text-muted-foreground">
                30 minutes free waiting after you land. Time for baggage claim, customs, and finding your driver stress-free.
              </p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <IndianRupee class="w-10 h-10 text-primary mb-2" />
              <CardTitle>Fixed ₹3,700 Rate</CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-sm text-muted-foreground">
                Zero surge pricing. Same ₹3,700 rate whether you land at 3 AM or 8 PM. All tolls and charges included.
              </p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <Users class="w-10 h-10 text-primary mb-2" />
              <CardTitle>7-Seater SUVs</CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-sm text-muted-foreground">
                Spacious luxury electric SUVs. Perfect for families or groups with multiple bags. Ample boot space for luggage.
              </p>
            </CardContent>
          </Card>
        </div>
      </div>
    </div>
  </section>

  <!-- Terminal-wise Pickup Guide -->
  <section class="w-full py-12 md:py-16 bg-muted/30">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto">
        <h2 class="text-3xl md:text-4xl font-bold text-center mb-4">Pickup Locations at Mumbai Airport</h2>
        <p class="text-center text-muted-foreground mb-12 max-w-2xl mx-auto">
          We pick up from all Mumbai airport terminals. Our driver will meet you at arrivals with a name board.
        </p>

        <div class="grid md:grid-cols-3 gap-6 mb-8">
          <Card>
            <CardHeader>
              <CardTitle className="flex items-center gap-2">
                <MapPin class="w-5 h-5 text-primary" />
                Terminal 1 (Domestic)
              </CardTitle>
            </CardHeader>
            <CardContent className="space-y-2">
              <p class="text-sm"><strong>Airlines:</strong> IndiGo, SpiceJet, Go First (some flights)</p>
              <p class="text-sm"><strong>Pickup Point:</strong> Arrivals exit, near Pillar 5-6</p>
              <p class="text-sm"><strong>What to Do:</strong> Exit arrivals, look for driver with name board near taxi stand area</p>
              <p class="text-sm text-primary font-semibold">Contact: +91 74982 64215</p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle className="flex items-center gap-2">
                <MapPin class="w-5 h-5 text-primary" />
                Terminal 2 (Intl & Domestic)
              </CardTitle>
            </CardHeader>
            <CardContent className="space-y-2">
              <p class="text-sm"><strong>Airlines:</strong> Air India, Vistara, International flights, Go First (most)</p>
              <p class="text-sm"><strong>Pickup Point:</strong> Arrivals Level 2, Gate 6-7 area</p>
              <p class="text-sm"><strong>What to Do:</strong> After baggage claim, exit to arrivals. Driver waits near Gate 6-7 with your name</p>
              <p class="text-sm text-primary font-semibold">Contact: +91 74982 64215</p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle className="flex items-center gap-2">
                <MapPin class="w-5 h-5 text-primary" />
                Navi Mumbai Airport
              </CardTitle>
            </CardHeader>
            <CardContent className="space-y-2">
              <p class="text-sm"><strong>New Airport:</strong> Opened 2024, Panvel area</p>
              <p class="text-sm"><strong>Pickup Point:</strong> Main arrivals exit</p>
              <p class="text-sm"><strong>Distance to Pune:</strong> 110 km via Pune-Mumbai Expressway (2-2.5 hours)</p>
              <p class="text-sm text-primary font-semibold">Same ₹3,700 rate!</p>
            </CardContent>
          </Card>
        </div>

        <Card className="bg-blue-50 border-blue-200">
          <CardContent className="pt-6">
            <h3 class="font-semibold mb-3 flex items-center gap-2">
              <Shield class="w-5 h-5 text-primary" />
              Can't Find Your Driver?
            </h3>
            <p class="text-sm mb-3">
              We send driver details 30 minutes before your flight lands: driver name, phone number, vehicle number. Driver will also have a name board with your name.
            </p>
            <p class="text-sm">
              <strong>If you can't locate driver:</strong>
            </p>
            <ul class="text-sm space-y-1 ml-4 list-disc">
              <li>Call driver directly (number shared on WhatsApp)</li>
              <li>Call our 24/7 support: +91 74982 64215</li>
              <li>WhatsApp us your location - we'll guide driver to you</li>
            </ul>
          </CardContent>
        </Card>
      </div>
    </div>
  </section>

  <!-- What's Included -->
  <section class="w-full py-12 md:py-16 bg-background">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto">
        <h2 class="text-3xl md:text-4xl font-bold text-center mb-12">What's Included in ₹3,700?</h2>

        <div class="grid md:grid-cols-2 gap-8">
          <div>
            <h3 class="text-xl font-semibold mb-4 flex items-center gap-2">
              <Check class="w-5 h-5 text-green-600" />
              Included Charges
            </h3>
            <ul class="space-y-3">
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Base Fare</p>
                  <p class="text-sm text-muted-foreground">150 km Mumbai Airport to Pune</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Toll Charges (₹430)</p>
                  <p class="text-sm text-muted-foreground">Mumbai-Pune Expressway tolls included</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Driver Allowance (₹500)</p>
                  <p class="text-sm text-muted-foreground">No separate driver charges or tips expected</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Airport Parking (₹70)</p>
                  <p class="text-sm text-muted-foreground">Parking charges for pickup included</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">GST & Taxes</p>
                  <p class="text-sm text-muted-foreground">All government taxes included</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Flight Tracking</p>
                  <p class="text-sm text-muted-foreground">Real-time monitoring, driver adjusts for delays</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">30 Minutes Free Waiting</p>
                  <p class="text-sm text-muted-foreground">After you land - no rush, no extra charges</p>
                </div>
              </li>
            </ul>
          </div>

          <div>
            <h3 class="text-xl font-semibold mb-4 flex items-center gap-2">
              <Zap class="w-5 h-5 text-green-600" />
              Service Features
            </h3>
            <ul class="space-y-3">
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Luxury 7-Seater Electric SUVs</p>
                  <p class="text-sm text-muted-foreground">Kia Carens EV or BYD eMax 7</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Professional Drivers</p>
                  <p class="text-sm text-muted-foreground">5+ years experience, background verified</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Zero Surge Pricing</p>
                  <p class="text-sm text-muted-foreground">Same ₹3,700 at 3 AM or peak hours</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">No Advance Payment</p>
                  <p class="text-sm text-muted-foreground">Pay after comfortable ride completion</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">Free Cancellation</p>
                  <p class="text-sm text-muted-foreground">Cancel up to 2 hours before pickup, no charges</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">GPS Tracking</p>
                  <p class="text-sm text-muted-foreground">Share live trip location with family/colleagues</p>
                </div>
              </li>
              <li class="flex items-start gap-3">
                <Check class="w-5 h-5 text-green-600 mt-0.5" />
                <div>
                  <p class="font-medium">24/7 Customer Support</p>
                  <p class="text-sm text-muted-foreground">WhatsApp/call support anytime</p>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Drop Locations in Pune -->
  <section class="w-full py-12 md:py-16 bg-muted/30">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto">
        <h2 class="text-3xl md:text-4xl font-bold text-center mb-4">Drop Anywhere in Pune - Same ₹3,700</h2>
        <p class="text-center text-muted-foreground mb-12 max-w-2xl mx-auto">
          No extra charges for specific Pune locations. We drop at your exact address in any Pune area.
        </p>

        <div class="grid md:grid-cols-3 gap-4">
          <Card>
            <CardHeader>
              <CardTitle className="text-lg">West Pune</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="text-sm space-y-2">
                <li>• <a href="/hinjewadi-to-mumbai-airport-cab" class="text-primary hover:underline">Hinjewadi</a> (IT Park, Phase 1/2/3)</li>
                <li>• <a href="/wakad-to-pune-airport-cab" class="text-primary hover:underline">Wakad</a></li>
                <li>• <a href="/baner-to-mumbai-cab-service" class="text-primary hover:underline">Baner</a> (Sus Road, Aundh Road)</li>
                <li>• Balewadi (High Street)</li>
                <li>• Pimple Saudagar</li>
                <li>• Pimple Nilakh</li>
                <li>• Aundh</li>
              </ul>
            </CardContent>
