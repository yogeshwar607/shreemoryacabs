---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Button } from "@/components/ui/button";
import { Badge } from "@/components/ui/badge";
import { MessageCircle, Phone } from "lucide-react";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "How much does a cab from Pune to Mumbai Airport cost?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "A cab from Pune to Mumbai Airport (Chhatrapati Shivaji Maharaj International Airport) costs ₹3,700 with Shree Morya Cabs. This is a fixed, all-inclusive rate covering tolls, parking, and driver charges. There's no surge pricing, even for early morning departures."
      }
    },
    {
      "@type": "Question",
      "name": "How long does it take to reach Mumbai Airport from Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "The journey from Pune to Mumbai Airport typically takes 2.5 to 3 hours via the Mumbai-Pune Expressway. During peak traffic, add 30-60 minutes buffer. We recommend leaving 4-4.5 hours before your flight."
      }
    },
    {
      "@type": "Question",
      "name": "How far is Mumbai Airport from Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Mumbai Airport is approximately 148 km from Pune city center via the Mumbai-Pune Expressway. The distance varies slightly from 140-155 km depending on your pickup location in Pune."
      }
    },
    {
      "@type": "Question",
      "name": "What time should I leave Pune for my Mumbai Airport flight?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "We recommend leaving Pune 4 hours before domestic flights and 4.5-5 hours before international flights. For a 6 AM flight, pickup should be at 1:30-2 AM. This accounts for travel time, check-in, security, and unexpected delays."
      }
    },
    {
      "@type": "Question",
      "name": "Do you charge extra for early morning pickups?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "No, there are no early morning or late-night surcharges with Shree Morya Cabs. Whether your pickup is at 2 AM or 2 PM, the fare remains ₹3,700. This saves you ₹3,000-8,000 compared to surge pricing on other platforms."
      }
    },
    {
      "@type": "Question",
      "name": "Do you provide drop to both Terminal 1 and Terminal 2?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, we provide drop service to both terminals. Terminal 1 (T1) for domestic flights on IndiGo, SpiceJet, etc., and Terminal 2 (T2) for all international flights and airlines like Vistara and Air India."
      }
    },
    {
      "@type": "Question",
      "name": "What type of vehicles are available?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Shree Morya Cabs operates 100% electric 7-seater premium SUVs: Kia Carens EV (400+ km range) and BYD E-MAX 7 (530+ km range). Both feature dual-zone AC, ample luggage space, USB charging, and silent electric operation."
      }
    },
    {
      "@type": "Question",
      "name": "Are there any hidden charges in the fare?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "No hidden charges. The ₹3,700 fare includes all expressway tolls (~₹400), Mumbai entry taxes, airport charges, driver allowance, luggage assistance, and GST. No early morning charges, night charges, or surge pricing ever."
      }
    },
    {
      "@type": "Question",
      "name": "How do I book a cab from Pune to Mumbai Airport?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Book via WhatsApp at +91 74982 64215 (recommended), phone call at the same number, or through shreemoryacabs.com. Share your flight details, terminal, pickup address, and time. We recommend booking 24 hours in advance."
      }
    },
    {
      "@type": "Question",
      "name": "Is it safe to travel at 2-3 AM for early morning flights?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, completely safe. All drivers are background-verified. We provide GPS tracking, trip sharing with family, 24/7 support, dashcam recording, and the Mumbai-Pune Expressway has regular police patrolling. Thousands travel safely with us for early flights every month."
      }
    }
  ]
};

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Pune to Mumbai Airport Cab FAQ", url: "/pune-to-mumbai-airport-cab-faq" }
]);
---

<MainLayout
  title="Pune to Mumbai Airport Cab FAQ: 50+ Questions Answered - Shree Morya Cabs"
  description="Complete FAQ about Pune to Mumbai Airport (BOM) cab service. Pricing, T1/T2 terminals, booking, routes, early morning pickups. Starting from ₹3,700 all-inclusive."
  keywords={[
    "pune to mumbai airport cab",
    "pune to bom taxi",
    "electric cab pune to mumbai airport",
    "mumbai airport drop service",
    "cab booking pune to terminal 1 terminal 2"
  ]}
>
  <StructuredData data={[faqSchema, breadcrumbSchema]} />

  <div class="py-12 md:py-16">
    <div class="container mx-auto px-4 max-w-6xl">
      <!-- Header -->
      <div class="text-center mb-12">
        <Badge className="mb-4 bg-orange-600 text-white">Complete FAQ Guide</Badge>
