                  <table class="w-full text-sm border-collapse">
                    <thead>
                      <tr class="border-b">
                        <th class="text-left p-2">Pickup Location</th>
                        <th class="text-left p-2">Distance to Mumbai Airport</th>
                      </tr>
                    </thead>
