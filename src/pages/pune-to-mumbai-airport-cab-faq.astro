        <!-- Section 3: Vehicle & Amenities -->
        <section>
          <h2 class="text-3xl font-bold mb-6 flex items-center gap-2">
            <span class="text-orange-600">03.</span> Vehicle & Amenities
          </h2>
          <div class="space-y-4">
            <Card>
              <CardHeader>
