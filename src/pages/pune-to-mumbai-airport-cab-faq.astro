                    <thead>
                      <tr class="border-b">
                        <th class="text-left p-2">Segment</th>
                        <th class="text-left p-2">Time</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr class="border-b">
                        <td class="p-2">Pune pickup to Expressway Entry</td>
                        <td class="p-2">20-40 mins</td>
                      </tr>
                      <tr class="border-b">
                        <td class="p-2">Mumbai-Pune Expressway</td>
                        <td class="p-2">1.5-2 hrs</td>
                      </tr>
                      <tr class="border-b">
                        <td class="p-2">Expressway Exit to Airport</td>
                        <td class="p-2">30-45 mins</td>
                      </tr>
