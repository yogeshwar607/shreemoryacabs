---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle, CardDescription } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Button } from "@/components/ui/button";
import Breadcrumbs from "@/components/seo/Breadcrumbs.astro";
import StructuredData from "@/components/seo/StructuredData.astro";
import { Building2, Check, Shield, FileText, Users, TrendingDown, Phone, MessageCircle, BarChart } from "lucide-react";

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "name": "Corporate Electric Cab Bookings",
  "serviceType": "B2B Electric Vehicle Transportation",
  "provider": {
    "@type": "Organization",
    "name": "GreenEVCabs",
    "telephone": "+91-7498264215"
  },
  "areaServed": {
    "@type": "State",
    "name": "Maharashtra"
  }
};
---

<MainLayout
  title="Corporate Electric Cab Bookings | Business Transport Solutions | GreenEVCabs"
  description="Premium corporate electric cab services for businesses in Maharashtra. Monthly packages, GST invoices, employee transportation, airport transfers. Trusted by 50+ companies. Volume discounts available."
  keywords={[
    "corporate cab bookings",
    "business cab service",
    "employee transportation",
    "corporate airport transfer",
    "monthly cab package",
    "gst invoice cab service",
    "corporate electric cab"
  ]}
>
  <Breadcrumbs items={[
    { name: "Home", href: "/" },
    { name: "Corporate Bookings", href: "/corporate" }
  ]} />

  <StructuredData data={[serviceSchema]} />

  <!-- Hero Section -->
  <section class="w-full py-12 md:py-24 bg-gradient-to-b from-primary/10 to-background">
    <div class="container px-4 md:px-6">
      <div class="text-center max-w-4xl mx-auto">
        <Badge variant="secondary" className="mb-4">
          <Building2 class="h-4 w-4 inline mr-2" />
          Trusted by 50+ Companies â€¢ GST Compliant
        </Badge>
        <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold tracking-tight mb-6">
          Corporate Electric Cab Bookings
        </h1>
        <p class="text-xl text-muted-foreground mb-8">
          Premium electric cab solutions for businesses. Monthly packages, GST invoices, dedicated support, and sustainable transportation for your team.
        </p>

        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href="https://wa.me/917498264215?text=Hi%2C%20I'm%20interested%20in%20corporate%20electric%20cab%20packages" target="_blank">
            <Button size="lg" className="bg-green-600 hover:bg-green-700">
              <MessageCircle className="mr-2 h-5 w-5" />
              Get Corporate Quote
            </Button>
          </a>
          <a href="tel:+917498264215">
            <Button size="lg" variant="outline">
              <Phone className="mr-2 h-5 w-5" />
              Call +91 74982 64215
            </Button>
          </a>
        </div>
      </div>
    </div>
  </section>

  <!-- Benefits Section -->
  <section class="w-full py-12 md:py-16">
    <div class="container px-4 md:px-6">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold tracking-tighter sm:text-4xl mb-4">
          Why Choose GreenEVCabs for Corporate Transportation?
        </h2>
        <p class="text-muted-foreground max-w-2xl mx-auto">
          Comprehensive business solutions designed for modern companies
        </p>
      </div>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6 max-w-6xl mx-auto">
        <Card>
          <CardHeader>
            <FileText class="h-10 w-10 text-primary mb-4" />
            <CardTitle>Monthly Billing & GST</CardTitle>
            <CardDescription>
              Consolidated monthly invoices with 18% GST. Easy expense management and accounting.
            </CardDescription>
          </CardHeader>
        </Card>

        <Card>
          <CardHeader>
            <TrendingDown class="h-10 w-10 text-primary mb-4" />
            <CardTitle>Volume Discounts</CardTitle>
            <CardDescription>
              Save 10-20% with our corporate packages. The more you travel, the more you save.
            </CardDescription>
          </CardHeader>
        </Card>

        <Card>
          <CardHeader>
            <Users class="h-10 w-10 text-primary mb-4" />
            <CardTitle>Dedicated Manager</CardTitle>
            <CardDescription>
              Your personal account manager for priority booking and 24/7 support.
            </CardDescription>
          </CardHeader>
        </Card>

        <Card>
          <CardHeader>
            <Shield class="h-10 w-10 text-primary mb-4" />
            <CardTitle>Professional Drivers</CardTitle>
            <CardDescription>
              Verified, trained drivers with 5+ years experience. Background checked and uniformed.
            </CardDescription>
          </CardHeader>
        </Card>

        <Card>
          <CardHeader>
            <BarChart class="h-10 w-10 text-primary mb-4" />
            <CardTitle>Travel Reports</CardTitle>
            <CardDescription>
              Detailed monthly reports with trip analytics, cost breakdowns, and carbon savings.
            </CardDescription>
          </CardHeader>
        </Card>

        <Card>
          <CardHeader>
            <Building2 class="h-10 w-10 text-primary mb-4" />
            <CardTitle>Sustainability Goals</CardTitle>
            <CardDescription>
              Help your company meet ESG targets with 100% electric, zero-emission fleet.
            </CardDescription>
          </CardHeader>
        </Card>
      </div>
    </div>
  </section>

  <!-- Services Section -->
  <section class="w-full py-12 md:py-16 bg-muted/50">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto">
        <h2 class="text-3xl font-bold mb-8 text-center">Corporate Services</h2>

        <div class="grid md:grid-cols-2 gap-6">
          <Card>
            <CardHeader>
              <CardTitle>Employee Transportation</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="space-y-2">
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Daily office pickup & drop services</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Shift-wise transportation for 24/7 operations</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Multi-location coverage across Pune & Mumbai</span>
                </li>
              </ul>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle>Airport Transfers</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="space-y-2">
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Business travel to Mumbai, Navi Mumbai, Pune airports</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Flight tracking for on-time pickups</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Executive meet & greet service</span>
                </li>
              </ul>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle>Client Transportation</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="space-y-2">
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Premium vehicles for client meetings</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Professional chauffeurs for VIP service</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Same-day booking available</span>
                </li>
              </ul>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle>Outstation Travel</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="space-y-2">
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Business trips to Shirdi, Nashik, Lonavala</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Team offsites and conferences</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="h-5 w-5 text-green-600 mt-0.5 flex-shrink-0" />
                  <span>Flexible multi-day packages</span>
                </li>
              </ul>
            </CardContent>
          </Card>
        </div>
      </div>
    </div>
  </section>

  <!-- CTA Section -->
  <section class="w-full py-12 md:py-16 bg-primary/10">
    <div class="container px-4 md:px-6">
      <div class="text-center max-w-3xl mx-auto">
        <h2 class="text-3xl font-bold mb-4">Ready to Get Started?</h2>
        <p class="text-muted-foreground mb-8">
          Join 50+ companies already saving costs and reducing emissions with GreenEVCabs corporate solutions.
        </p>
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href="https://wa.me/917498264215?text=Hi%2C%20I'm%20interested%20in%20corporate%20packages.%20Please%20share%20details." target="_blank">
            <Button size="lg" className="bg-green-600 hover:bg-green-700">
              Request Quote on WhatsApp
            </Button>
          </a>
          <a href="/corporate-electric-cab-pune">
            <Button size="lg" variant="outline">
              View Detailed Corporate Solutions
            </Button>
          </a>
        </div>
      </div>
    </div>
  </section>
</MainLayout>
