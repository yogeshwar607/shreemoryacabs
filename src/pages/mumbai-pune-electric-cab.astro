---
import MainLayout from "@/layouts/MainLayout.astro";
import { Button } from "@/components/ui/button";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Check, MapPin, Zap, IndianRupee, Clock, Users, Shield } from "lucide-react";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "serviceType": "Electric Cab Service Mumbai to Pune",
  "provider": {
    "@type": "LocalBusiness",
    "name": "Shree Morya Cabs",
    "description": "Reliable electric cab service from Mumbai to Pune with professional drivers and luxury 7-seater SUVs at ₹2,700"
  },
  "areaServed": [
    {
      "@type": "City",
      "name": "Mumbai, Maharashtra, India"
    },
    {
      "@type": "City",
      "name": "Pune, Maharashtra, India"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2700",
    "priceCurrency": "INR"
  }
};

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "How much does electric cab cost from Mumbai to Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Electric cab from Mumbai to Pune costs ₹2,700 with Shree Morya Cabs. This includes all tolls, parking, driver charges, and GST. Zero hidden fees or surge pricing."
      }
    },
    {
      "@type": "Question",
      "name": "Which is the best electric cab for Mumbai to Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Shree Morya Cabs is the best electric cab for Mumbai to Pune with ₹2,700 fixed pricing, luxury 7-seater SUVs (Kia Carens EV & BYD eMax 7), professional drivers, and zero surge pricing. Save ₹500 compared to competitors."
      }
    },
    {
      "@type": "Question",
      "name": "How long does it take from Mumbai to Pune by electric cab?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Electric cab from Mumbai to Pune takes approximately 2.5 to 3 hours covering 148 km via Mumbai-Pune Expressway, depending on traffic conditions and your exact pickup and drop locations."
      }
    }
  ]
};

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Mumbai to Pune Electric Cab", url: "/mumbai-pune-electric-cab" }
]);
---

<MainLayout
