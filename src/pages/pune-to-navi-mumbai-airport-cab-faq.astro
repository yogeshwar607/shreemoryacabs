---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Button } from "@/components/ui/button";
import { Badge } from "@/components/ui/badge";
import { MessageCircle, Phone } from "lucide-react";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "How much does a cab from Pune to Navi Mumbai Airport cost?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "A cab from Pune to Navi Mumbai International Airport (NMIA) costs ₹3,700 with GreenEVCabs. This is a fixed, all-inclusive rate covering tolls, parking, and driver charges. There's no surge pricing, even for early morning departures."
      }
    },
    {
      "@type": "Question",
      "name": "How long does it take to reach Navi Mumbai Airport from Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "The journey from Pune to Navi Mumbai Airport typically takes 2 to 2.5 hours via the Mumbai-Pune Expressway. During peak traffic hours, add 30-45 minutes buffer for Pune city traffic."
      }
    },
    {
      "@type": "Question",
      "name": "How far is Navi Mumbai Airport from Pune?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Navi Mumbai International Airport (NMIA) is approximately 120 km from Pune city center via the Mumbai-Pune Expressway. The distance varies slightly depending on your pickup location in Pune."
      }
    },
    {
      "@type": "Question",
      "name": "What time should I leave Pune for my flight from NMIA?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "We recommend leaving Pune 3.5 hours before your domestic flight and 4 hours before international flights. This accounts for travel time, check-in, and security. For a 6 AM flight, pickup should be at 2:30 AM."
      }
    },
    {
      "@type": "Question",
      "name": "Do you charge extra for early morning pickups?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "No, there are no early morning or late-night surcharges with GreenEVCabs. Whether your pickup is at 3 AM or 3 PM, the fare remains ₹3,700. Early morning flights are our specialty."
      }
    },
    {
      "@type": "Question",
      "name": "What type of vehicles are available?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "GreenEVCabs operates 100% electric 7-seater premium SUVs: Kia Carens EV (400+ km range) and BYD E-MAX 7 (530+ km range). Both feature climate control, ample luggage space, and silent electric operation."
      }
    },
    {
      "@type": "Question",
      "name": "Are there any hidden charges in the fare?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "No hidden charges. The ₹3,700 fare includes all expressway tolls, driver allowance, luggage assistance, and GST. There are no early morning charges, night charges, or surge pricing. The price you see is the price you pay."
      }
    },
    {
      "@type": "Question",
      "name": "How do I book a cab from Pune to Navi Mumbai Airport?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Book via WhatsApp at +91 74982 64215 (recommended), phone call at the same number, or through greenevcabs.com. Share your flight details, pickup address, and departure time. We recommend booking 24 hours in advance."
      }
    },
    {
      "@type": "Question",
      "name": "Is it safe to travel at 3-4 AM for early morning flights?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, completely safe. All drivers are background-verified. We provide GPS tracking, trip sharing with family, 24/7 support, and dashcam recording. Thousands of passengers travel safely for early morning flights with us."
      }
    },
    {
      "@type": "Question",
      "name": "Which is closer from Pune - NMIA or Mumbai Airport?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Navi Mumbai Airport (NMIA) is closer at 120 km and takes 2-2.5 hours. Mumbai Airport (BOM) is 148 km and takes 2.5-3 hours. NMIA is 28 km closer and 30-45 minutes faster from most Pune locations."
      }
    }
  ]
};

const breadcrumbSchema = generateBreadcrumbSchema([
  { name: "Home", url: "/" },
  { name: "Pune to Navi Mumbai Airport Cab FAQ", url: "/pune-to-navi-mumbai-airport-cab-faq" }
]);
---

<MainLayout
  title="Pune to Navi Mumbai Airport Cab FAQ: 50+ Questions Answered - GreenEVCabs"
  description="Complete FAQ about Pune to Navi Mumbai Airport cab service. Pricing, booking, routes, timing, early morning pickups. Starting from ₹2,300 all-inclusive fare."
  keywords={[
    "pune to navi mumbai airport cab",
    "pune to nmia taxi",
    "electric cab pune to navi mumbai airport",
    "navi mumbai airport drop service",
    "cab booking pune nmia"
  ]}
>
  <StructuredData data={[faqSchema, breadcrumbSchema]} />

  <div class="py-12 md:py-16">
    <div class="container mx-auto px-4 max-w-6xl">
      <!-- Header -->
      <div class="text-center mb-12">
        <Badge className="mb-4 bg-blue-600 text-white">Complete FAQ Guide</Badge>
