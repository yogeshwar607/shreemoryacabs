---
import MainLayout from "@/layouts/MainLayout.astro";
import { Button } from "@/components/ui/button";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Check, MapPin, Zap, Shield, Clock, IndianRupee, Users, Leaf } from "lucide-react";
import StructuredData from "@/components/seo/StructuredData.astro";

const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "serviceType": "Electric Taxi Service Pune to Mumbai",
  "provider": {
    "@type": "LocalBusiness",
    "name": "GreenEVCabs",
    "description": "Premium electric taxi service from Pune to Mumbai with professional drivers and luxury 7-seater SUVs at ₹2,700"
  },
  "areaServed": [
    {
      "@type": "City",
      "name": "Pune, Maharashtra, India"
    },
    {
      "@type": "City",
      "name": "Mumbai, Maharashtra, India"
    }
  ],
  "offers": {
    "@type": "Offer",
    "price": "2700",
    "priceCurrency": "INR"
  }
};

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "What is the cost of electric taxi from Pune to Mumbai?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Electric taxi from Pune to Mumbai costs ₹2,700 with GreenEVCabs. This includes all tolls, parking, driver charges, and GST. Zero hidden fees or surge pricing."
      }
    },
    {
      "@type": "Question",
      "name": "Which is the best electric taxi service for Pune-Mumbai?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "GreenEVCabs is the best electric taxi service for Pune-Mumbai with ₹2,700 fixed pricing, professional drivers, luxury 7-seater SUVs (Kia Carens EV & BYD E-MAX 7), and zero surge pricing. Save ₹500 compared to competitors."
      }
    },
    {
      "@type": "Question",
      "name": "How long does electric taxi take from Pune to Mumbai?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Electric taxi from Pune to Mumbai takes approximately 2.5 to 3 hours covering 148 km via Mumbai-Pune Expressway, depending on traffic conditions and pickup location."
      }
    },
    {
      "@type": "Question",
      "name": "Are electric taxis cheaper than diesel cabs for Pune-Mumbai?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, electric taxis are significantly cheaper. GreenEVCabs charges ₹2,700 while diesel cabs cost ₹5,000+. You save ₹1,300 per trip by choosing electric taxi service."
      }
    }
  ]
};
