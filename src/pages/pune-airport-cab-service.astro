    "telephone": "+917498264215"
  },
  "areaServed": {
    "@type": "City",
    "name": "Pune, Maharashtra, India"
  },
  "offers": [
    {
      "@type": "Offer",
      "name": "Pune Airport to City Center",
      "price": "500",
      "priceCurrency": "INR",
      "description": "Pune Airport to Shivajinagar, Deccan areas"
