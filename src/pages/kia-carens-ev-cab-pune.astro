---
import MainLayout from "@/layouts/MainLayout.astro";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Button } from "@/components/ui/button";
import Breadcrumbs from "@/components/seo/Breadcrumbs.astro";
import StructuredData from "@/components/seo/StructuredData.astro";
import { Check, Zap, Shield, Users, Leaf, Star, Phone, MessageCircle, Battery } from "lucide-react";
---

<MainLayout
  title="Kia Carens EV Cab Pune ₹2,999 | 7 Seater Electric SUV Rental | Book Now"
  description="Book Kia Carens EV cab in Pune at ₹2,999 for Mumbai Airport. 7-seater electric SUV with 400+ km range, ADAS safety, spacious interiors. Best rates for Kia Carens electric cab rental. Available 24/7."
  keywords={[
    "kia carens ev cab pune",
    "kia carens electric cab",
    "kia carens ev rental pune",
    "7 seater kia carens cab",
    "kia carens ev taxi pune",
    "book kia carens ev",
    "kia carens cab pune to mumbai",
    "kia carens electric suv cab"
  ]}
>
  <Breadcrumbs items={[
    { name: "Home", href: "/" },
    { name: "Kia Carens EV Cab Pune", href: "/kia-carens-ev-cab-pune" }
  ]} />

  <div class="container px-4 py-12 md:py-16">
    <!-- Hero -->
    <div class="text-center mb-12">
