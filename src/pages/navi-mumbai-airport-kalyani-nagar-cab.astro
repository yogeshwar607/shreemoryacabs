---
import MainLayout from "@/layouts/MainLayout.astro";
import { Button } from "@/components/ui/button";
import { Card } from "@/components/ui/card";
import StructuredData from "@/components/seo/StructuredData.astro";
import { generateBreadcrumbSchema } from "@/lib/seo";

const serviceSchema = { "@context": "https://schema.org", "@type": "Service", "provider": { "@type": "LocalBusiness", "name": "Shree Morya Cabs", "telephone": "+917498264215" }, "offers": { "price": "2300", "priceCurrency": "INR" } };
const breadcrumbSchema = generateBreadcrumbSchema([{ name: "Home", url: "/" }, { name: "NMIA", url: "/navi-mumbai-airport-to-pune-cab" }, { name: "Kalyani Nagar", url: "/navi-mumbai-airport-kalyani-nagar-cab" }]);
---

<MainLayout title="NMIA to Kalyani Nagar Cab Starting ₹2,300 | Premium Residential Area" description="Navi Mumbai Airport to Kalyani Nagar Starting ₹2,300. Upscale residential area. 130 km. Book now!" keywords={["nmia kalyani nagar"]}>
  <StructuredData data={[serviceSchema, breadcrumbSchema]} />
  <section class="py-20 bg-green-50 dark:bg-green-900/30">
    <div class="container mx-auto px-4 text-center">
      <h1 class="text-5xl font-bold mb-6">NMIA to Kalyani Nagar Starting ₹2,300</h1>
      <p class="text-xl mb-8">130 km | Premium Residential | IT Offices</p>
      <Button size="lg" className="bg-green-600" asChild><a href="tel:+917498264215">Book Now</a></Button>
    </div>
  </section>
</MainLayout>
