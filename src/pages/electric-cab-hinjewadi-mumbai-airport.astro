---
import MainLayout from "@/layouts/MainLayout.astro";
import { Button } from "@/components/ui/button";
import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card";
import { Badge } from "@/components/ui/badge";
import { Check, MapPin, Clock, IndianRupee } from "lucide-react";
import StructuredData from "@/components/seo/StructuredData.astro";

// Service schema for this specific route
const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "serviceType": "Electric Cab Service from Hinjewadi to Mumbai Airport",
  "provider": {
    "@type": "LocalBusiness",
    "name": "Shree Morya Cabs",
    "telephone": "+91-74982-64215"
  },
  "areaServed": {
    "@type": "City",
    "name": "Hinjewadi, Pune"
  },
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "Hinjewadi to Mumbai Airport Service",
    "itemListElement": [
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Kia Carens EV - Hinjewadi to Mumbai Airport"
        },
        "price": "2700",
        "priceCurrency": "INR"
      }
    ]
  }
};
---

<MainLayout
  title="Electric Cab Hinjewadi to Mumbai Airport ₹2,700 | IT Park Pickup | Shree Morya Cabs"
  description="Best electric cab from Hinjewadi IT Park to Mumbai Airport at ₹2,700. Friendly, experienced drivers, 7-seater SUVs, pickup from Phase 1/2/3. Corporate packages available. Book 24/7."
  keywords={[
    "hinjewadi to mumbai airport cab",
    "electric cab hinjewadi",
    "hinjewadi it park to mumbai airport",
    "hinjewadi phase 1 mumbai airport",
    "hinjewadi phase 2 mumbai airport cab",
    "it park electric cab",
    "corporate cab hinjewadi mumbai"
  ]}
>
  <StructuredData data={[serviceSchema]} />

  <!-- Hero Section -->
  <section class="w-full py-12 md:py-24 bg-gradient-to-b from-orange-50 dark:from-orange-950/20 to-background">
    <div class="container px-4 md:px-6">
      <div class="max-w-4xl mx-auto">
        <Badge variant="secondary" className="mb-4">
          Hinjewadi IT Park Specialist • Corporate Packages Available
        </Badge>
        <h1 class="text-3xl font-bold tracking-tighter sm:text-4xl md:text-5xl mb-4">
          Electric Cab from Hinjewadi to Mumbai Airport
        </h1>
        <p class="text-xl text-muted-foreground mb-6">
          Reliable electric cab service from all Hinjewadi IT Parks (Phase 1, 2, 3) to Mumbai Airport at ₹2,700. Friendly, experienced drivers, zero surge pricing, perfect for IT professionals.
        </p>

        <div class="grid md:grid-cols-3 gap-4 mb-8">
          <div class="flex items-center gap-2 text-lg">
            <IndianRupee class="w-5 h-5 text-orange-600 dark:text-orange-400" />
            <span class="font-semibold">₹2,700 Fixed Rate</span>
          </div>
          <div class="flex items-center gap-2 text-lg">
            <MapPin class="w-5 h-5 text-orange-600 dark:text-orange-400" />
            <span class="font-semibold">155 km</span>
          </div>
          <div class="flex items-center gap-2 text-lg">
            <Clock class="w-5 h-5 text-orange-600 dark:text-orange-400" />
            <span class="font-semibold">3-3.5 hours</span>
          </div>
        </div>

        <div class="flex flex-col sm:flex-row gap-4">
          <a href="https://wa.me/917498264215?text=I%20want%20to%20book%20a%20cab%20from%20Hinjewadi%20to%20Mumbai%20Airport" target="_blank">
            <Button size="lg" className="bg-orange-600 hover:bg-orange-700">
              Book on WhatsApp
            </Button>
          </a>
          <a href="tel:+917498264215">
            <Button size="lg" variant="outline">
              Call +91 74982 64215
            </Button>
          </a>
        </div>
      </div>
    </div>
  </section>

  <!-- IT Park Coverage -->
  <section class="w-full py-12 md:py-16 bg-muted/30">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto">
        <h2 class="text-3xl font-bold text-center mb-8">Pickup from All Hinjewadi IT Parks</h2>

        <div class="grid md:grid-cols-3 gap-6">
          <Card>
            <CardHeader>
              <CardTitle>Hinjewadi Phase 1</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="space-y-2 text-sm">
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Wipro Campus</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>TCS Campus</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Infosys Campus</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Tech Mahindra</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>All Phase 1 locations</span>
                </li>
              </ul>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle>Hinjewadi Phase 2</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="space-y-2 text-sm">
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Cognizant Campus</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Accenture Campus</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Capgemini</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Persistent Systems</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>All Phase 2 locations</span>
                </li>
              </ul>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle>Hinjewadi Phase 3</CardTitle>
            </CardHeader>
            <CardContent>
              <ul class="space-y-2 text-sm">
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Rajiv Gandhi Infotech Park</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>EON Free Zone</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Blue Ridge</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>Residential areas</span>
                </li>
                <li class="flex items-start gap-2">
                  <Check class="w-4 h-4 text-green-600 mt-0.5" />
                  <span>All Phase 3 locations</span>
                </li>
              </ul>
            </CardContent>
          </Card>
        </div>
      </div>
    </div>
  </section>

  <!-- Why Choose Us for Hinjewadi -->
  <section class="w-full py-12 md:py-16">
    <div class="container px-4 md:px-6">
      <div class="max-w-5xl mx-auto">
        <h2 class="text-3xl font-bold text-center mb-8">Why IT Professionals Choose Shree Morya Cabs</h2>

        <div class="grid md:grid-cols-2 gap-6">
          <Card>
            <CardHeader>
              <CardTitle className="flex items-center gap-2">
                <Check class="w-5 h-5 text-green-600" />
                Early Morning Pickups
              </CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-muted-foreground">
                Catch that 6 AM flight? We offer early morning pickups from 3 AM onwards. Friendly, experienced drivers ensure you reach the airport on time, every time.
              </p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle className="flex items-center gap-2">
                <Check class="w-5 h-5 text-green-600" />
                Corporate GST Invoice
              </CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-muted-foreground">
                Get GST invoices for expense claims. Perfect for business travel and company reimbursements. Monthly billing available for corporate accounts.
              </p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle className="flex items-center gap-2">
                <Check class="w-5 h-5 text-green-600" />
                Zero Surge Pricing
              </CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-muted-foreground">
                ₹2,700 fixed rate 24/7. No surge during peak hours, early mornings, or weekends. What you see is what you pay - always.
              </p>
            </CardContent>
          </Card>

          <Card>
            <CardHeader>
              <CardTitle className="flex items-center gap-2">
                <Check class="w-5 h-5 text-green-600" />
                Monthly Packages
              </CardTitle>
            </CardHeader>
            <CardContent>
              <p class="text-muted-foreground">
                Frequent traveler? Get discounted monthly packages. Perfect for IT professionals with regular Mumbai business trips or airport commutes.
              </p>
            </CardContent>
          </Card>
        </div>
      </div>
    </div>
  </section>

  <!-- Route Information -->
  <section class="w-full py-12 md:py-16 bg-muted/30">
    <div class="container px-4 md:px-6">
      <div class="max-w-4xl mx-auto">
        <h2 class="text-3xl font-bold text-center mb-8">Route & Timing Details</h2>

        <Card>
          <CardContent className="pt-6">
            <div class="space-y-4">
              <div>
                <h3 class="font-semibold text-lg mb-2">Best Route</h3>
                <p class="text-muted-foreground">
                  Hinjewadi → Mumbai-Pune Expressway → Mumbai Airport (Terminal 1 & 2)
                </p>
              </div>

              <div>
                <h3 class="font-semibold text-lg mb-2">Distance & Duration</h3>
                <ul class="space-y-2 text-muted-foreground">
                  <li>• Total Distance: 155 km</li>
                  <li>• Normal Traffic: 3-3.5 hours</li>
                  <li>• Peak Hours: 3.5-4 hours</li>
                  <li>• Early Morning: 2.5-3 hours</li>
                </ul>
              </div>

              <div>
                <h3 class="font-semibold text-lg mb-2">Recommended Buffer Time</h3>
                <ul class="space-y-2 text-muted-foreground">
                  <li>• Domestic Flights: Book 4 hours before flight</li>
                  <li>• International Flights: Book 4.5-5 hours before flight</li>
                  <li>• Includes check-in time and possible traffic delays</li>
                </ul>
              </div>

              <div>
                <h3 class="font-semibold text-lg mb-2">What's Included in ₹2,700</h3>
                <ul class="space-y-2 text-muted-foreground">
                  <li class="flex items-center gap-2">
                    <Check class="w-4 h-4 text-green-600" />
                    All toll charges (Mumbai-Pune Expressway + Mumbai tolls)
                  </li>
                  <li class="flex items-center gap-2">
                    <Check class="w-4 h-4 text-green-600" />
                    Airport parking charges
                  </li>
                  <li class="flex items-center gap-2">
                    <Check class="w-4 h-4 text-green-600" />
                    Friendly driver charges
                  </li>
                  <li class="flex items-center gap-2">
                    <Check class="w-4 h-4 text-green-600" />
                    Premium 7-seater electric SUV
                  </li>
                  <li class="flex items-center gap-2">
                    <Check class="w-4 h-4 text-green-600" />
                    Flight tracking (no extra charge for delays)
                  </li>
                </ul>
              </div>
            </div>
          </CardContent>
        </Card>
      </div>
    </div>
  </section>

  <!-- Booking CTA -->
  <section class="w-full py-12 md:py-16 bg-orange-50 dark:bg-orange-950/20">
    <div class="container px-4 md:px-6">
      <div class="max-w-3xl mx-auto text-center">
        <h2 class="text-3xl font-bold mb-4">Ready to Book Your Hinjewadi-Mumbai Airport Cab?</h2>
        <p class="text-xl text-muted-foreground mb-6">
          ₹2,700 fixed rate | Zero surge | Corporate GST invoice available
        </p>
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href="https://wa.me/917498264215?text=I%20want%20to%20book%20from%20Hinjewadi%20to%20Mumbai%20Airport" target="_blank">
            <Button size="lg" className="bg-orange-600 hover:bg-orange-700">
              Book on WhatsApp
            </Button>
          </a>
          <a href="tel:+917498264215">
            <Button size="lg" variant="outline">
              Call Now
            </Button>
          </a>
        </div>
        <p class="mt-4 text-sm text-muted-foreground">
          Available 24/7 | Pickup from all IT parks | Friendly, experienced drivers
        </p>
      </div>
    </div>
  </section>
</MainLayout>
